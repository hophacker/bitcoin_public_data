<script type="text/javascript">
    jQuery(document).ready(function(j) {
        var set_placeholder = function(el) {
            el.val(el.data('placeholder') || '');
        };
        
        j(document).on('focus', '.global-header-search input', function() {
            if (j.trim(j(this).val()) === j(this).data('placeholder')) {
                j(this).val('');
            }
        }).on('blur', '.global-header-search input', function() {
            if (j.trim(j(this).val()) === '') {
                set_placeholder(j(this));
            }
        });
        
        j('.global-header-search input').focus().blur();

        var mp_props = {};

        if (PP.login != '') {
            mp_props['user'] = PP.login;
            mp_props['distinct_id'] = PP.user_id;
        } else {
            mp_props['user'] = 'anonymous_user';
        }
        mixpanel.track_links(".global-header-explore-broadcast", "dropdown_broadcast", mp_props);
        mixpanel.track_links(".global-header-broadcast", "broadcast_topbar", mp_props);


        var cookieDomain = '.justin.tv';
        var cookieOptions = {
            'path': '/',
            'domain': cookieDomain
        }
        var savedAlertID = j.cookie('saved_alert_id')
        var siteAlertID = null;

        // If saved_alert_id cookie is different from the current site alert id,
        // a new alert should be seen by user, therefore, update show_alert cookie
        // and the saved_alert_id to proper values
        if (savedAlertID && siteAlertID && savedAlertID !== siteAlertID) {
            j.cookie('show_alert', 'true', cookieOptions);
            j.cookie('saved_alert_id', 'None', cookieOptions);
        }

        // Hide alert upon clicking X on alert and update necessary cookies
        j('.global-alert-close').click(function() {
            j('.global-alert').hide();
            j.cookie('show_alert', 'false', cookieOptions);
            j.cookie('saved_alert_id', 'None', cookieOptions);
        });
        
    });
</script>



