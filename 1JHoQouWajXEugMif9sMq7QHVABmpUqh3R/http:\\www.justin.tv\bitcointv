<script>

    // Comscore Tag
    document.write(unescape(
            "%3Cscript src='" + 
            (document.location.protocol == "https:" ? "https://sb" : "http://b") + 
            ".scorecardresearch.com/beacon.js' %3E%3C/script%3E"
    ));

    // Send beacon on timeout
    var beacon_sent = false;

    setTimeout(function(){
        if (!beacon_sent && typeof COMSCORE !== 'undefined') {
            beacon_sent = true;
            var sourceCookie = readCookie("psHashedUserId");
            COMSCORE.beacon({
                c1: 2,
                c2: 6745306,
                c3: 0,
                c4: "",
                c5: "01",
                c6: "",
                c15: (sourceCookie === null || sourceCookie == "CXCXCXCX") ? "" : sourceCookie
            });
        }
    }, 3000);

</script>

