    </script>


    <script>
        // workaround for chrome v33 fonts bug
        jQuery(document).ready(function() {
            jQuery('body')
                .delay(500)
                .animate({padding: '0 1px'}, 100)
                .delay(500)
                .animate({padding: '0'}, 100);
        });
    </script>
