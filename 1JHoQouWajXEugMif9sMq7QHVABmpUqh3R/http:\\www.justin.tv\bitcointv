    // for pro ad AB testing 
    jQuery(document).ready(function() {
        var pro_banner_ad = jQuery('#pro_banner_ad');
        if (pro_banner_ad.length > 0){
            pro_banner_ad.click(function(e){
                _gaq.push(['_trackEvent', 'Pro Banner Ad', 'Click', jQuery(this).data('banner-ad')]);
            });
            
        }
    });
</script>


