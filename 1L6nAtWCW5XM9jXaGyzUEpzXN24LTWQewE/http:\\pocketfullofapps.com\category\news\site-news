var imgs2 = new Array("http://i.imgur.com/nIb2OBt.png","http://i.imgur.com/nIb2OBt.png");
var lnks2 = new Array("http://bit.ly/1lAb3qZ","http://bit.ly/1lAb3qZ");
var alt2 = new Array("Caribbean Catch for iOS","Caribbean Catch iOS");
var currentAd2 = 0;
var imgCt2 = 2;
function cycle2() {
  if (currentAd2 == imgCt2) {
    currentAd2 = 0;
  }
var banner2 = document.getElementById('adBanner2');
var link2 = document.getElementById('adLink2');
  banner2.src=imgs2[currentAd2]
  banner2.alt=alt2[currentAd2]
  document.getElementById('adLink2').href=lnks2[currentAd2]
  currentAd2++;
}
  window.setInterval("cycle2()",6000);
</script>
