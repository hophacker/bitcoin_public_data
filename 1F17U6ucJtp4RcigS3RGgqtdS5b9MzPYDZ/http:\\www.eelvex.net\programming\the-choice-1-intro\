<span class="co2">#define N 2 //Dimensions</span><br />
<span class="co2">#define x0 0.0</span><br />
<span class="co2">#define x1 3.0</span><br />
<span class="co2">#define dt 0.01</span><br />
<br />
<span class="kw4">double</span> <span class="sy0">*</span>solution<span class="br0">&#91;</span>N<span class="br0">&#93;</span> <span class="sy0">=</span> <span class="br0">&#123;</span>NULL<span class="br0">&#125;</span><span class="sy0">;</span><br />
<br />
<span class="kw4">void</span> deriv1 <span class="br0">&#40;</span><span class="kw4">double</span> x<span class="sy0">,</span> <span class="kw4">double</span> y<span class="br0">&#91;</span><span class="br0">&#93;</span><span class="sy0">,</span> <span class="kw4">double</span> dydt<span class="br0">&#91;</span><span class="br0">&#93;</span><span class="br0">&#41;</span><br />
<span class="br0">&#123;</span><br />
&nbsp; &nbsp; dydt<span class="br0">&#91;</span><span class="nu0">0</span><span class="br0">&#93;</span> <span class="sy0">=</span> <span class="nu0">1</span> <span class="sy0">-</span> y<span class="br0">&#91;</span><span class="nu0">1</span><span class="br0">&#93;</span><span class="sy0">;</span><br />
&nbsp; &nbsp; dydt<span class="br0">&#91;</span><span class="nu0">1</span><span class="br0">&#93;</span> <span class="sy0">=</span> y<span class="br0">&#91;</span><span class="nu0">0</span><span class="br0">&#93;</span> <span class="sy0">-</span> x<span class="sy0">;</span><br />
<span class="br0">&#125;</span><br />
<br />
<span class="kw4">int</span> main <span class="br0">&#40;</span><span class="kw4">void</span><span class="br0">&#41;</span><br />
<span class="br0">&#123;</span><br />
&nbsp; &nbsp; <span class="kw4">int</span> i<span class="sy0">;</span><br />
&nbsp; &nbsp; <span class="kw4">int</span> steps<span class="sy0">;</span> <span class="co1">// solution steps</span><br />
&nbsp; &nbsp; <span class="kw4">double</span> starting_v<span class="br0">&#91;</span>N<span class="br0">&#93;</span><span class="sy0">;</span> <span class="co1">// Starting vector</span><br />
<br />
&nbsp; &nbsp; steps <span class="sy0">=</span> <span class="br0">&#40;</span>x1<span class="sy0">-</span>x0<span class="br0">&#41;</span><span class="sy0">/</span>dt<span class="sy0">;</span><br />
<br />
&nbsp; &nbsp; <span class="kw1">for</span> <span class="br0">&#40;</span>i<span class="sy0">=</span><span class="nu0">0</span><span class="sy0">;</span>i<span class="sy0">&lt;</span>N<span class="sy0">;</span>i<span class="sy0">++</span><span class="br0">&#41;</span><br />
&nbsp; &nbsp; &nbsp; &nbsp; solution<span class="br0">&#91;</span>i<span class="br0">&#93;</span> <span class="sy0">=</span> <span class="br0">&#40;</span><span class="kw4">double</span> <span class="sy0">*</span><span class="br0">&#41;</span><a href="http://www.opengroup.org/onlinepubs/009695399/functions/realloc.html"><span class="kw3">realloc</span></a><span class="br0">&#40;</span><span class="br0">&#40;</span><span class="kw4">char</span> <span class="sy0">*</span><span class="br0">&#41;</span>solution<span class="br0">&#91;</span>i<span class="br0">&#93;</span><span class="sy0">,</span> <span class="kw4">sizeof</span><span class="br0">&#40;</span><span class="kw4">double</span><span class="br0">&#41;</span><span class="sy0">*</span>steps<span class="br0">&#41;</span><span class="sy0">;</span><br />
<br />
&nbsp; &nbsp; <span class="coMULTI">/* -- initial values -- */</span><br />
&nbsp; &nbsp; starting_v<span class="br0">&#91;</span><span class="nu0">0</span><span class="br0">&#93;</span> <span class="sy0">=</span> <span class="nu16">1.0</span><span class="sy0">;</span><br />
&nbsp; &nbsp; starting_v<span class="br0">&#91;</span><span class="nu0">1</span><span class="br0">&#93;</span> <span class="sy0">=</span> <span class="nu16">0.0</span><span class="sy0">;</span><br />
<br />
&nbsp; &nbsp; rkdump<span class="br0">&#40;</span>starting_v<span class="sy0">,</span> N<span class="sy0">,</span> x0<span class="sy0">,</span> x1<span class="sy0">,</span> steps<span class="sy0">,</span> deriv1<span class="br0">&#41;</span><span class="sy0">;</span><br />
<br />
&nbsp; &nbsp; <span class="kw1">for</span> <span class="br0">&#40;</span>i<span class="sy0">=</span><span class="nu0">0</span><span class="sy0">;</span>i<span class="sy0">&lt;</span>steps<span class="sy0">;</span>i<span class="sy0">++</span><span class="br0">&#41;</span> <span class="br0">&#123;</span><br />
&nbsp; &nbsp; &nbsp; &nbsp; <a href="http://www.opengroup.org/onlinepubs/009695399/functions/printf.html"><span class="kw3">printf</span></a><span class="br0">&#40;</span><span class="st0">&quot;%4f %4f %4f<span class="es1">\n</span>&quot;</span><span class="sy0">,</span> <span class="br0">&#40;</span>i<span class="sy0">+</span><span class="nu0">1</span><span class="br0">&#41;</span><span class="sy0">*</span>dt<span class="sy0">,</span> solution<span class="br0">&#91;</span><span class="nu0">0</span><span class="br0">&#93;</span><span class="br0">&#91;</span>i<span class="br0">&#93;</span><span class="sy0">,</span> solution<span class="br0">&#91;</span><span class="nu0">1</span><span class="br0">&#93;</span><span class="br0">&#91;</span>i<span class="br0">&#93;</span><span class="br0">&#41;</span><span class="sy0">;</span><br />
&nbsp; &nbsp; <span class="br0">&#125;</span><br />
<br />
&nbsp; &nbsp; <span class="kw1">return</span> <span class="nu0">0</span><span class="sy0">;</span><br />
<span class="br0">&#125;</span></div></div>
