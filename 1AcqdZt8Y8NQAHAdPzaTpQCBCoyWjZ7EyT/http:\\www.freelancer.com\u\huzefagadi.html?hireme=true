<style>
.password_strength {
padding: 0 3px;
display: inline-block;
}
.password_strength_1 {
    background-color: #fcb6b1;
}
.password_strength_2 {
    background-color: #fccab1;
}
.password_strength_3 {
    background-color: #fcfbb1;
}
.password_strength_4 {
    background-color: #dafcb1;
}
.password_strength_5 {
    background-color: #bcfcb1;
}
</style>

<script src="https://cdn2.f-cdn.com/js/jquery.password.js?4&amp;v=0cc582501495cb44d2b81fa941fd1f22&amp;m=2" type="text/javascript"></script>

<script type="text/javascript">
    jq= jQuery.noConflict();
    jq('#passwd').password_strength({
            'container':jq('#passwd_hint_id'),
            'invalidMessage': 'Password must have at least 6 characters.',
            'checkValidation': function () {
                if (jq('#passwd').val().length > 6 && !isValidPassword()) {
                    return false;
                }
                else {
                    return true;
                }
            }
        });

    function isValidPassword() {
        var hint_msg = /^.{6,}$/i;
        if (hint_msg.test(jq('#passwd').val())) {
            return true;
        }
        return false;
    }

    function isPasswordStrongEnough() {
        var hint_msg = /weak/i;
        if (hint_msg.test(jq('#passwd_hint_id').html())) {
            return false;
        }
        return true;
    }

    function validate() {
        var msg = null;
        var res = true;

        if (!isValidPassword()){
            msg=((msg==null)?'':msg+"\n")+'Password must have at least 6 characters.';
            res=false;
        }

        if (document.getElementById('passwd').value!='' && !isPasswordStrongEnough()) {
            msg=((msg==null)?'':msg+"\n")+'Please make sure you enter a stronger password (at least Average level)';
            document.getElementById('passwd').focus();
            res=false;
        }

        if (msg!=null)
            alert(msg);

        return res;
    }
</script>


<style>
.ns_error {float:left;}
.ns_form #err-placeholder-period.ns_error {background:#ffffff!important; color:inherit !important;}
.icon-green-tick {width:16px;height:16px;background:url('/img/sprite-icons.png?46');background-position:-32px -5362px;float:left;padding-right:10px;}
#returnuser_welcomeback {color: #008BCB; padding-bottom:10px;font-size:18px;font-weight:normal;}
</style>
<script type="text/javascript">
require([
    'jquery-wrapper',
    'select2-v3.4.2',
    'lib/misc/AC_OETags'
], function(jQuery) {
    var jq = jQuery;
    window.json_min_budget = {"fixed":{"1":"10","2":"14","3":"10","4":"10","5":"80","6":"12","7":"440","8":"8","9":"10","10":"100","11":"600","12":"900","13":"5000","14":"140","15":"100000","16":"32","17":"70","18":"1000","19":"30"},"hourly":{"1":"2","2":"3","3":"2","4":"2","5":"16","6":"3","7":"90","8":"2","9":"2","10":"16","11":"100","12":"180","13":"1000","14":"30","15":"20000","16":"6.6","17":"12.8","18":"200","19":"6"}};
    window.is_auth = false;
    window.IP = "54.82.170.223";
    window.hireMeReady=false;
    window.projectNameIsNotDuplicated = true;
    window.lastProjectNameChecked = '';
    window.feeNotes = false;

    

    window.getHiremeUpgradePrice = function(id) {
        jq.ajax({
            url:'/ajax/hireme/getHiremeUpgradePrice.php',
            data:{
                id: id,
                userId: userId
            },
            dataType:'json',
            success: function(resp){
                if(resp.status == 'success'){
                    window.featured_price = resp.result;
                    window.monthlyHiremeProjectRecievedNumber = resp.monthlyHiremeProjectRecievedNumber;
                    jq('#featured_price').html(resp.result);
                    jq('#monthlyHiremeProjectRecievedNumber').html(resp.monthlyHiremeProjectRecievedNumber);
                    if(monthlyHiremeProjectRecievedNumber >= 50){
                        jq('#featuredUpgradePopularFreelancerText').show();
                        jq('#featuredUpgradeNormalText').hide();
                    }
                }
            }
        });
    }


    //Should only ever run once
    window.prepareHireMe = function(){
        if(hireMeReady)return;
        else hireMeReady=true;

        window.HireMeProject = Backbone.Model.extend({
            defaults: function() {
                var budget_period = 'fixed';
                var budget = 25;
                var descr = null;
                
                var currency_id = 1 ;
                var refp_defined = '';
                var refp_budget = refp_defined ? budget < 30 ? 30 : budget : 0;

                if(refp_defined){
                   var min_fixed_budget = refp_budget;
                } else {
                   var min_fixed_budget = json_min_budget['fixed'][currency_id]*15;
                }
                window.featured_price = 0;
                getHiremeUpgradePrice(currency_id);

                return {
                    "username": null,
                    "all_skills":null,
                    "project_name":"",
                    "descr":descr,
                    "period":4,
                    "budget": budget,
                    "min_fixed_budget": min_fixed_budget,
                    "hourlyrate": "25",
                    "budget_period": budget_period,
                    "currency_id":1 ,
                    "currency_sign": '$',
                    "min_budget":json_min_budget[budget_period]['1']
                        ,
                    "commitment":null,
                    "commitment_interval":'week',
                    "duration":2,
                    "is_online":false,
                    "returnuser_loggedin":false,
                    "returnuser_username":false
                };
            },

            autoProjectName: function() {
                var myDate = new Date();
                var qh_date = myDate.toDateString().split(' ');
                var qh_time = myDate.toTimeString().split(' ');
                return 'Custom Project' + myDate.toDateString().replace(qh_date[0],"")+ " " + qh_time[0];
            }
        });
        window.HireMeProjectView = Backbone.View.extend({
            template: _.template(jq('#hireme-form-template').html()),
            initialize: function() {
              this.model.bind('change', this.render, this);
            },
            render: function() {
                if(jq('#fileupload-container').has('#normalUploadFileView').length !== 0){
                    jq('#normalUploadFileView').appendTo(jq('#normalUploadFileViewWrapper'));
                }
                
                
                jq('#hireme-form-container').show().html(this.template(this.model.toJSON()));
                jq('#hireme-form-container .currency').val(this.model.get("currency_id"));

                jq('#initialMilestoneCurrency').html(jq('.currency:visible: option:selected').text().trim());

                if ( !is_auth ) {
                    init_password_plugin();
                }

                jq('#normalUploadFileViewWrapper #normalUploadFileView').appendTo(jq('#fileupload-container'));

                return this;
            }
        });
        window.hireMeProject = new HireMeProject;
        window.hireMeProjectView = new HireMeProjectView({model:hireMeProject});
    }

    window.initialSkillSelect = function(values){
        if(values){
            jq('#skill_select').val(values);
        }
        jq('#skill_select').select2({
            placeholder:T_('Enter a skill...'),
            maximumSelectionSize:5}
        ).on("change", function(e){
            updateSkillSelectCount();
        });
    }

    window.updateSkillSelectCount = function() {
        var max = 5;
        var selected = jq('#skill_select').val();
        var length = 0;
        if(selected){
            length = selected.length;
        }

        jq('#qh_skill_err').html('');

        var remain = max - length;
        if ( remain == 0 ) {
            jq('#skill_select_count').html(T_('(You cannot add more skills)'));
        } else if ( remain == 1 ) {
            jq('#skill_select_count').html(T_('(You can add 1 more skill)'));
        } else {
            jq('#skill_select_count').html(T_('(You can add <[-remain]> more skills)',{remain:remain}));
        }
    }

    window.showPopoverTips = function() {

        jq('#creatInitialMilestone-tip').popover_legacy({
            template : '<div class="popover"><div class="arrow"></div><div class="popover-inner" style="width:300px"><div class="popover-content"><p></p></div></div></div>',
            content: T_('The Milestone Payment System is our protected payments feature. It allows <[-username]> to start work, knowing funds are in the system, and gives you peace of mind because you only release the funds once you are 100% satisfied. You can divide your project into smaller tasks (milestones) and pay <[-username]> in stages as each milestone is completed.',{username:username}) ,
            html: true,
        });

        jq('body').on('mouseenter', '#autoOpenToBidding-tip', function() {
          jq(this).popover_legacy({
            template : '<div class="popover"><div class="arrow"></div><div class="popover-inner" style="width:400px"><h3 class="popover-title"></h3><div class="popover-content"><p></p></div></div></div>',
                trigger: 'hover',
                html: true,
                placement: 'top'
          });
          jq(this).popover_legacy('show');
        });

        jq('body').on('mouseenter','#project-type-tip',function(){
            jq(this).popover_legacy({
              content:
                T_('<strong>Fixed Price</strong>: Ideal when you have a project with a well defined scope and detailed project description. '+
                   'Project fees are charged when the freelancer accepts the project.') + '<br><br>' +
                T_('<strong>Hourly Projects</strong>: Recommended when you want to hire a freelancer for ongoing work. ' +
                   'The project description can be less well defined. Project fees are charged on each payment to the freelancer.'),
               trigger: 'hover',
               html: true
            });
            jq(this).popover_legacy('show');
        });

        jq('body').on('mouseenter','.quick-hire-post-form-tip:not("#project-type-tip")',function(){
            jq(this).popover_legacy('show');
        });
        jq('body').on('mouseleave','.quick-hire-post-form-tip',function(){
            jq(this).popover_legacy('hide');
        });
    }

    window.showFixedBudgetSuggestion = function() {
        var initialed = false;
        var initialFixBudgetSuggetion = function(){
            if(typeof fixedBudgetSuggestion !== 'undefined'){
                var content = T_('<%= userName %> has accepted similar projects for $<%= fixedBudgetSuggestion %> USD',
                    {userName: "huzefagadi",fixedBudgetSuggestion: fixedBudgetSuggestion});
                jq("#qh_fixed_budget").popover_legacy({
                    title: "Not sure on the Budget?",
                    content: content,
                    trigger: "manual",
                    html: true
                });
                if(!initialed){
                    initialed = true;
                    jq("#qh_fixed_budget").val(fixedBudgetSuggestion);
                }
            }
        };
        var showFixBudgetSuggestion = function(){
            initialFixBudgetSuggetion();
            jq("#qh_fixed_budget").popover_legacy("show");
        }
        var hideFixBudgetSuggestion = function(){
            initialFixBudgetSuggetion();
            jq("#qh_fixed_budget").popover_legacy("hide");
        }
        var toggleFixBudgetSuggestion = function(){
            initialFixBudgetSuggetion();
            jq("#qh_fixed_budget:visible").popover_legacy("toggle");
        }

        jq.ajax({
            url:'/ajax/hireme/getFixedProjectBudgetSuggestion.php?user_id=1900974',
            dataType:'json',
            success: function(resp){
                if(resp.status == 'success' && resp.result != null && resp.result < 500 && resp.result > 300){
                    window.fixedBudgetSuggestion = resp.result;
                    initialFixBudgetSuggetion();
                }
            }
        });
        jq('body').on('focus','#qh_fixed_budget',showFixBudgetSuggestion);
        jq('body').on('blur','#qh_fixed_budget',hideFixBudgetSuggestion);
        jq('body').on('toggleBudgetSuggestion','input[type=radio][name=project-type][value=fixed]',toggleFixBudgetSuggestion);
        jq('body').on('click','input[type=radio][name=project-type][value=hourly]',hideFixBudgetSuggestion);
    }

    window.check_username_existence_passed = false;
    window.check_email_existence_passed = false;
    window.ajax_req_username = false;
    window.ajax_req_email = false;
    window.ajax_req_login = false;
    window.check_existing_user_passed = false;
    window.is_username_error = function() {
        if(jq('#tbx_username').length != 0) {
            var pattern=/^[a-z][a-z0-9]{2,15}$/i
            if(!pattern.test(jq('#tbx_username').val())) {
                return true;
            } else {
                return false;
            }
        }
        return false;
    }
    window.validate_existing_user = function() {
        if ( jQuery.trim(jq('#username_txt').val()) == '' || jQuery.trim(jq('#passwd_txt').val()) == '' ) {
            displayErrorMessage({
                element: '#username_txt',
                errorMessage: T_('Please enter your login details')
            });
            jq('#passwd_txt').parent('.control-group').addClass('error');
            check_existing_user_passed = false;
            return false;
        }
        var username = jq('#username_txt').val();
        return jq.post(ssl_base_url + '/ajax/login.php', {username:username, passwd:jq('#passwd_txt').val()},
            function(json) {
                if (json.status == 'success') {
                    check_existing_user_passed = true;
                    discardErrorMessage({ element: '#username_txt' });
                    jq('#passwd_txt').parent('.control-group').removeClass('error');
                    jq('fieldset.login_choice').fadeOut('slow');
                    jq('#login-form').fadeOut('slow');
                    jq('#returnuser_username').html(username);
                    jq('#returnuser_welcomeback').fadeIn('slow');
                    jq('#hireme-post-project-header').remove();
                    jq('.loggedout_header').fadeOut('slow');
                    pinkyHeaderAPI.registerCallback(reRenderFeeNotes);
                    pinkyHeaderAPI.reload();
                } else {
                    check_existing_user_passed = false;
                    displayErrorMessage({ element: '#username_txt', errorMessage: T_('Invalid login details') });
                    jq('#passwd_txt').parent('.control-group').addClass('error');
                }
            }, 'jsonp'
        );
    }

    window.reRenderFeeNotes = function() {
        if(feeNotes == false) {
            fetchFeeNotes();
        } else {
            updateFeeNotes();
        }
    }
    window.updateFeeNotes = function() {
        if(feeNotes) {
            if (feeNotes.defaultFee == 0 && feeNotes.defaultFeePercent == 0) {
                jq('#project_fee_placeholder').remove();
            } else {
                jq(".default_fee_percent").text(feeNotes.defaultFeePercent);
                jq(".default_fee").text(feeNotes.defaultCurrency.sign + feeNotes.defaultFee + " " + feeNotes.defaultCurrency.code);
            }
        }
    }
    window.fetchFeeNotes = function() {
        jq.ajax({
            type: 'get',
            data: {},
            dataType: 'json',
            url: '/ajax/hireme/getUserProjectFeeDetails.php',
            success: function(response) {
                if (response.status == 'success') {
                    feeNotes = response.fee;
                    updateFeeNotes();
                } else {
                    /* keep old fee notes and do nothing*/
                }
            },
            error: function() {/* keep old fee notes and do nothing*/}
        });
    }

    window.username_validate = function() {

        if ( is_username_error() ) {
            displayErrorMessage({
                element: '#tbx_username',
                errorMessage: T_('Please enter 3-16 alphanumeric characters [a-z 0-9] starting with a letter')
            });
            return false;
        } else if ( !check_username_existence_passed ) {
            displayErrorMessage({
                element: '#tbx_username',
                errorMessage: T_('Username already exists')
            });
            return false;
        } else {
            discardErrorMessage({ element: '#tbx_username' });
            return true;
        }
    }


    window.checkUsernameExistence = function(username) {
        if ( ajax_req_username ) {
            ajax_req_username.abort();
        }
        if(!is_username_error()) {
            jq("#check_username_exist").html('');
            return jq.ajax({
                url: '/ajax/check-username-exist.php?username='+username+'&json=true',
                success: function(returnData) {
                    var json = jq.parseJSON(returnData);
                    if (json.result == 'success') {
                        jq("#check_username_exist").html(json.msg+'&nbsp;<img class="ns_pad-none" src="https://cdn5.f-cdn.com/img/icons/icon_check.png?v=c9f8cedbddf533af820e0414bc4943d6&amp;m=2" style="height: 13px;" />');
                        check_username_existence_passed = true;
                    } else {
                        jq("#check_username_exist").html('');
                        check_username_existence_passed = false;
                    }
                    username_validate();
                }
            });
        } else {
            jq("#check_username_exist").html('');
            username_validate();
        }
        return false;
    }

    window.is_email_error = function() {
        if (jq('#tbx_email').length != 0) {
            if(/^([\w\-\+!#%\*\=\?\^\{\}\|~]+(?:\.[\w\-\+!#%\*\=\?\^\{\}\|~]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i.test(jq('#tbx_email').val())) {
                return false;
            } else {
                return true;
            }
        }
        return false;
    }

    window.email_validate = function() {

        if ( is_email_error() ) {
            displayErrorMessage({
                element: '#tbx_email',
                errorMessage: T_('Please input a valid email')
            });
            return false;
        } else if ( !check_email_existence_passed ) {
            displayErrorMessage({
                element: '#tbx_email',
                errorMessage: T_('Email exists')
            });
            return false;
        } else {
            discardErrorMessage({ element: '#tbx_email' });
            return true;
        }

    }

    window.checkEmailExistence = function(email) {
        if ( ajax_req_email ) {
            ajax_req_email.abort();
        }
        if (!is_email_error()) {
            return jq.post('/ajax/check-email-exist.php?json=true', {email:email}, function(json) {
                    if (json.result == 'success') {
                        jq("#check_email_exist").html(json.msg+'&nbsp;<img class="ns_pad-none" src="https://cdn5.f-cdn.com/img/icons/icon_check.png?v=c9f8cedbddf533af820e0414bc4943d6&amp;m=2" style="height: 13px;" />');
                        autofillUsername(email);
                        check_email_existence_passed = true;
                    } else {
                        jq("#check_email_exist").html('');
                        check_email_existence_passed = false;
                    }
                    email_validate();
                }, 'json');
        } else {
            jq("#check_email_exist").html('');
            email_validate();
        }
        return false;
    }

    window.autofillUsername = function(initalemail){
        var suggestUsername = initalemail.indexOf('@') == -1 ? initalemail : initalemail.substring(0, initalemail.indexOf('@'));
        suggestUsername = suggestUsername.replace(/[^a-z0-9\s]/gi, '').replace(/[_\s]/g, '');
        if (suggestUsername.length > 16){
            suggestUsername = suggestUsername.substring(0, 16);
        }
        var currentUsername = jQuery('#tbx_username').val();

        if (currentUsername == '' && suggestUsername != '' && suggestUsername.length >= 3) {
          jQuery('#tbx_username').val(suggestUsername);
          checkUsernameExistence(suggestUsername);
        }
    }

    // return true is password error appears
    window.isPasswordErr = function() {
        var position = {
            top: -10,
            left: 235
        };
        jq('#passwd_match_msg').hide();

        if ( jQuery.trim(jq('#passwd').val()) == '') {
            displayErrorMessage({
                element: '#passwd',
                errorMessage: T_('Please enter your password')
            });
            return true;
        }
        if(!isPasswordStrongEnough()) {
            displayErrorMessage({
                element: '#passwd',
                errorMessage: jq('#passwd_hint_id').html()
            });
            return true;
        }

        if (jQuery('#tbx_username').val() === jQuery('#passwd').val()) {
            displayErrorMessage({
                element: '#passwd',
                errorMessage: T_('Password must not be equal to username')
            });
            return true;
        }

        jq('#passwd_match_msg').show();
        return false;
    }

    window.init_password_plugin = function() {
        jq('#passwd').password_strength({
            'container':jq('#passwd_hint_id'),
            'invalidMessage': 'Password must have at least 6 characters',
            'checkValidation': function () {
               if (jq('#passwd').val().length > 6 && !isValidPassword()) {
                    return false;
               }
               else{
                    return true;
               }
            }
        });
    }

    window.glb_model = {};
    window.save_current_state = function() {
        glb_model.selected_skills = [];
        glb_model.project_name = jq('#qh_name').val();
        glb_model.project_descr = jq('#qh_descr').val();
        glb_model.new_username = jq('#tbx_username').val();
        glb_model.new_email = jq('#tbx_email').val();
        glb_model.new_passwd = jq('#passwd').val();
        glb_model.new_passwd1 = jq('#passwd1').val();
        glb_model.login_choice = jq('input[name=login_choice]:checked').val();
        glb_model.existing_username = jq('#username_txt').val();
        glb_model.existing_passwd = jq('#passwd_txt').val();
        glb_model.email_notify = jq('#chkbox_email_notify').attr('checked');
        glb_model.returnuser_username = jq('#returnuser_username').html();
        glb_model.autoOpenToBidding = jq('#autoOpenToBidding').is(':checked');

        glb_model.qh_fixed_budget = jq('#qh_fixed_budget').val();
        glb_model.qh_period = jq('#qh_period').val();
        glb_model.qh_hourly_budget = jq('#qh_hourly_budget').val();
        glb_model.qh_hourly_commitment = jq('#qh_hourly_commitment').val();
        if(jq('input[name="hours_option"]:visible').length>0){
            glb_model.hours_option = jq('input[name="hours_option"]:checked').val();
        }
        glb_model.qh_hourly_duration = jq('#qh_hourly_duration').val();

        glb_model.pmb = jq('#pmb').val();
        glb_model.skill_select = jq('#skill_select').val();
    }
    window.restore_state = function() {
        jq('#tbx_username').val(glb_model.new_username);
        jq('#tbx_email').val(glb_model.new_email);
        jq('#passwd').val(glb_model.new_passwd);
        jq('#passwd1').val(glb_model.new_passwd1);
        jq('#chkbox_email_notify').attr('checked', glb_model.email_notify);
        jq('input[name=login_choice][value='+glb_model.login_choice+']').attr('checked', true);
        jq('input[name=login_choice]').trigger('change');
        jq('#username_txt').val(glb_model.existing_username);
        jq('#passwd_txt').val(glb_model.existing_passwd);
        jq('#qh_name').val(glb_model.project_name);
        jq('#qh_descr').val(glb_model.project_descr);
        jq('#returnuser_username').html(glb_model.returnuser_username);
        jq('#autoOpenToBidding').attr('checked', glb_model.autoOpenToBidding);

        jq('#qh_fixed_budget').val(glb_model.qh_fixed_budget);
        jq('#qh_period').val(glb_model.qh_period);
        jq('#qh_hourly_budget').val(glb_model.qh_hourly_budget);
        jq('#qh_hourly_commitment').val(glb_model.qh_hourly_commitment);
        jq('input[name="hours_option"][value='+glb_model.hours_option+']').click();
        jq('#qh_hourly_duration').val(glb_model.qh_hourly_duration);
        jq('#pmb').val(glb_model.pmb);

        initialSkillSelect(glb_model.skill_select);

    }

    window.initiaNoFlashFileUpload = function() {
        jq("<div><input type=\"file\" name=\"evidence[]\" /><input class=\"remove-upload\" type=\"button\" value=\"-\" /></div> <div id=\"div-add-upload\"><input id=\"add-upload\" type=\"button\" value=\"+\" /></div>").insertAfter("#qh_description");
        jq("#divProjectUploadBox").remove();
        jq("#upload_completedMessagePool").remove();
        jq("#add-upload").click(function() {
            jq("<div><input type=\"file\" name=\"evidence[]\" /><input class=\"remove-upload\" type=\"button\" value=\"-\" /></div>").insertBefore("#div-add-upload");
        });
        jq(".remove-upload").live('click', function() {
            jq(this).parent('div').remove();
        });
    }

    window.SWFUPL_fileAlreadyUploaded = function(id, filename) {
        var samefile = false;
        jq('#'+id+'_files > option').each(function() {
            if(jq(this).html() == filename) {
                samefile = true;
            }
        });
        if(samefile) {
            return "You have already uploaded this file.";
        }
        return false;
    }

    window.limitText = function(txtField, limitNum) {
        if(txtField.val().length > limitNum) {
            txtField.val(txtField.val().substr(0, limitNum));
        }
    }

    window.validate = function() {
        var type = hireMeProject.get("budget_period");
        var qh_name_error = qh_name_validate();
        var qh_descr_error = qh_descr_validate();
        var qh_skills_error = qh_skills_validate();

        if(qh_name_error){
            qh_name_duplicate_validate();
        }

        var validate_user_info = true;
        if ( !is_auth && jq('input[name=login_choice]:checked').val() == 'new' ) {
            var validate_username = username_validate();
            var validate_passwd = !isPasswordErr();
            var validate_email = email_validate();
            validate_user_info = validate_username && validate_passwd && validate_email;
        } else if ( !is_auth && jq('input[name=login_choice]:checked').val() == 'existing' ) {
            validate_user_info = check_existing_user_passed;
            if ( jQuery.trim(jq('#username_txt').val()) == '' || jQuery.trim(jq('#passwd_txt').val()) == '' ) {
                displayErrorMessage({
                    element: '#username_txt',
                    errorMessage: T_('Please enter your login details')
                });
                jq('#passwd_txt').parent('.control-group').addClass('error');
            }
        }

        if ( type == 'fixed' ) {
            var qh_period_error = qh_period_validate();
            var qh_budget_error = qh_fixed_budget_validate();

            return validate_user_info && qh_name_error && qh_descr_error && qh_period_error && qh_budget_error && qh_skills_error &&projectNameIsNotDuplicated;
        } else {
            var qh_commitment_error = qh_hourly_commitment_validate();
            var qh_budget_error = qh_hourly_budget_validate();

            return validate_user_info && qh_name_error && qh_descr_error && qh_commitment_error && qh_budget_error && qh_skills_error && projectNameIsNotDuplicated;
        }
    }
    window.qh_name_validate = function() {

        if(jq.trim(jq('#qh_name').val()).length > 255) {
            displayErrorMessage({
                element: '#qh_name',
                errorMessage: T_('Your project name is too long (maximum of 255 characters).')
            });
            return false;
        }

        if(jq.trim(jq('#qh_name').val()).length < 10) {
            displayErrorMessage({
                element: '#qh_name',
                errorMessage: T_('Enter a project name (at least 10 characters).')
            });
            return false;
        } else {
            discardErrorMessage({ element: '#qh_name' });
            return true;
        }
    }

    window.qh_name_duplicate_validate = function(){
        if(lastProjectNameChecked == jq('#qh_name').val()){
            show_qh_name_duplicate_error();
            return ;
        }
        jq.ajax({
            url: '/ajax/hireme/checkDuplicateProjectName.php',
            data: {
                name: jq('#qh_name').val()
            },
            type: 'post',
            dataType: 'json',
            beforeSend: function(){
                lastProjectNameChecked = jq('#qh_name').val();
            },
            success: function(resp){
                if(resp && resp.status == 'success'){
                    window.projectNameIsNotDuplicated = !resp.duplicated;
                    show_qh_name_duplicate_error();
                };
            }
        });
    }

    window.show_qh_name_duplicate_error = function(){
        if(!projectNameIsNotDuplicated){
            displayErrorMessage({
                element: '#qh_name',
                errorMessage: T_('Duplicate project. You have already posted a project with this name.')
            });
        }
    }

    window.qh_descr_validate = function() {
        var limit = jq('#autoOpenToBidding').is(':checked') ? 100 : 10;
        var upperLimit = 2000;

        if(jq.trim(jq('#qh_descr').val()).length >= upperLimit - 10) {
            displayErrorMessage({
                element: '#qh_descr',
                errorMessage: T_('Maximum characters for description is nearly reached.')
            });
            return false;
        }

        if(jq.trim(jq('#qh_descr').val()).length < limit) {
            displayErrorMessage({
                element: '#qh_descr',
                errorMessage: T_('Enter a project description (at least <[-limit]> characters).',{limit: limit})
            });
            return false;
        } else {
            discardErrorMessage({ element: '#qh_descr' });
            return true;
        }
    }
    window.qh_period_validate = function() {

        var periodDom = jq('#qh_period');

        if (periodDom.val().length > 10) {
            displayErrorMessage({
                element: '#qh_period',
                errorMessage: T_('Invalid value.')
            });
            return false;
        }

        var pattern=/^[1-9][0-9]{0,10}$/i;
        if(!pattern.test(periodDom.val())) {
            displayErrorMessage({
                element: '#qh_period',
                errorMessage: T_('Invalid number of days.')
            });
            return false;
        } else {
            discardErrorMessage({ element: '#qh_period' });
            return true;
        }
    }
    window.qh_fixed_budget_validate = function() {

        var budgetDom = jq('.fixed-project-container input.budget');

        if (budgetDom.val().length > 10) {
            displayErrorMessage({
                element: '#qh_fixed_budget',
                errorMessage: T_('Invalid value.')
            });
            return false;
        }

        var pattern=/^[1-9][0-9]{0,10}$/i;
        var currency_id = hireMeProject.get("currency_id");
        var currency_sign = hireMeProject.get("currency_sign");
        var budget = parseInt(budgetDom.val());
        var min_budget = json_min_budget['fixed'][currency_id];

        if (!pattern.test(budget) || parseInt(budget) < parseInt(min_budget)) {
            displayErrorMessage({
                element: '#qh_fixed_budget',
                errorMessage: T_("Project budget must be higher than <[-currency]><[-limit]>", { currency: currency_sign, limit: min_budget })
            });
            return false;
        } else {
            discardErrorMessage({ element: '#qh_fixed_budget' });
            return true;
        }
    }
    window.qh_hourly_budget_validate = function() {

        var budgetDom = jq('.hourly-project-container input.budget');

        if (budgetDom.val().length > 10) {
            displayErrorMessage({
                element: '#qh_hourly_budget',
                errorMessage: T_('Invalid value.')
            });
            return false;
        }

        var pattern=/^[1-9][0-9]{0,10}$/i;
        var currency_id = hireMeProject.get("currency_id");
        var currency_sign = hireMeProject.get("currency_sign");
        var budget = parseInt(budgetDom.val());
        var min_budget = json_min_budget['hourly'][currency_id];

        if (!pattern.test(budget) || budget < parseInt(min_budget)) {
            displayErrorMessage({
                element: '#qh_hourly_budget',
                errorMessage: T_("Project budget must be higher than <[-currency]><[-limit]>", { currency: currency_sign, limit: min_budget })
            });
            return false;
        } else {
            discardErrorMessage({ element: '#qh_hourly_budget' });
            return true;
        }
    }

    window.qh_skills_validate = function() {
        var selected = jq('#skill_select').val();
        var len = 0;
        if(selected){
            len = selected.length;
        }
        if ( len < 1 || len > 5 ) {
            jq('#qh_skill_err').html("<font color='#CD3F0C'>Please select at least 1 skill</font>");
            return false;
        } else {
            jq('#qh_skill_err').html('');
            return true;
        }
    }

    window.qh_hourly_commitment_validate = function() {

            var pattern=/^[1-9][0-9]{0,10}$/i;
            if(!pattern.test(jq('.hourly-project-container input.commitment').val())) {
                displayErrorMessage({
                    element: '#qh_hourly_commitment',
                    errorMessage: T_('Invalid number of hours per week')
                });
                return false;
            } else {
                discardErrorMessage({ element: '#qh_hourly_commitment' });
                return true;
            }
        }

    window.transferFundsToken = "84cb7849d2cf41bc0409269d181d082a28dc37cb";
    (function() {
        window.get_hireme_return_params = function() { return false; };
    })();
    window.submit_form = function() {
        var skills = jq('#skill_select').val();
        var type = hireMeProject.get("budget_period");

        //temporary fix for non-object reference when window.fileupload is not initialised
        try {
            var filename = _.keys(window.fileupload.uploaded_files);
        } catch (e) {}

        var post_var = {name:jq('#qh_name').val(),
                        descr:jq('#qh_descr').val(),
                        budget:jq('.'+type+'-project-container input.budget').val(),
                        currency:jq('.'+type+'-project-container select.currency').val(),
                        skills:skills,
                        filename: filename,
                        fileset: window.fileupload.fileset,
                        type:type,
                        autoOpenToBidding:jq('#autoOpenToBidding').attr('checked'),
                        initialMilestoneAmount: jq('#initialMilestoneAmount').html(),
                        pmb:jq('#pmb').val(),
                        featured: jq('#featured').attr('checked')};
        if ( jq('input[name=login_choice]:checked').val() == 'new' ) {
            post_var.username = jq('#tbx_username').val();
            post_var.email = jq('#tbx_email').val();
            post_var.passwd = jq('#passwd').val();
            post_var.passwd1 = jq('#passwd').val();
            post_var.email_notify = jq('#chkbox_email_notify').attr('checked');

            }

            if ( type == 'fixed' ) {
                post_var.period = jq('#qh_period').val();
            } else {
                post_var.commitment = jq('.'+type+'-project-container input.commitment').val();
                post_var.commitment_interval = jq('.'+type+'-project-container .commitment_interval').val();
                post_var.duration = jq('.'+type+'-project-container .duration').val();
            }
            if ( getQueryStringValueByName('ext') != '' ) {
                post_var.ext = 1;
            }

            if ( typeof custom_post_var == 'function' ) {
                post_var = custom_post_var(post_var);
            }

        var submit_btn = jq('.btn-hire-now');
        var cancel_btn = jq('.btn-cancel');
        var similar_freelancer = jq('#find-similar-freelancer-link');
        var loading_img = submit_btn.nextAll('.loadingImg:first');
        submit_btn.hide();
        cancel_btn.hide();
        similar_freelancer.hide();
        loading_img.show();
        jq.post('/buyers/onquickhire.php', post_var, function(r) {
            loading_img.hide();
            submit_btn.show();
            cancel_btn.show();
            if (r.status == 'success') {
                _lt.push({'event': 'HireMe Posted'});
                
                _ttref.push(['_trackEventNoCookie', 'HireMe', 'GoalEvent', 'profilePage', 'success_hireme_project']);
                
                pinkyHeaderAPI.reload();
                window.scrollTo(0,0);
                jQuery(window).unbind('beforeunload');
                if ( typeof submit_form_callback == 'function' ) {
                    submit_form_callback(r);
                }
                get_hireme_return_params = function() {
                    return {
                        seller_username:r.seller_username,
                        seller_id:r.seller_id,
                        project_id: r.project_id,
                        currency_sign: r.currency_sign,
                        currency_code: r.currency_code,
                        currency_id: r.currency_id,
                        currency_exchangerate: r.currency_exchangerate,
                        balance: r.balance,
                        url: r.url
                    };
                };
                show_milestone_form(r.seller_username, parseFloat(post_var.budget), r.currency_sign, post_var, r.currency_code);
            } else {
                if(typeof r.suggestion != 'undefined'){
                    similar_freelancer.attr('href', r.suggestion);
                    jQuery(window).unbind('beforeunload');
                    similar_freelancer.show();
                }
                alert(r.reason);
            }
        }, 'json');
    }

    window.show_milestone_form = function(seller_username, budget, currency_sign,post_var, currency_code) {
        jq('.container-milestone').remove();
        if (HiremeABTest.milestoneFormPercentageTest()) {
            jq('.hireme-form').after(jq(_.template(jq('#create-milestone-form-template').html(), {seller_username:seller_username, amount:budget, currency_sign:currency_sign,post_var:post_var, currency_code:currency_code, test_form:true})));
            var budget = post_var.budget;
            jq('.milestone-hour-count').on('keyup', {budget: budget}, function(){
                var count = parseInt(jq(this).val()) > 0 ? parseInt(jq(this).val()) : 0;
                jq('.milestone-amount').text((budget*count).toFixed(2));
                if(count > 1) {
                    jq('.input-append .add-on').text(T_("hrs"));
                }
            });
            jq('.milestone-percentage').on('keyup', {budget: budget}, function(){
                var percentage = parseInt(jq(this).val()) > 0 ? parseInt(jq(this).val()) : 0;
                jq('.milestone-amount').text((budget*percentage/100).toFixed(2));
            })
        } else {
            jq('.hireme-form').after(jq(_.template(jq('#create-milestone-form-template').html(), {seller_username:seller_username, amount:budget, currency_sign:currency_sign,post_var:post_var, currency_code:currency_code, test_form:false})));
        }

        jq('.hireme-form').slideUp();

        window.fb_param = {};
        fb_param.pixel_id = '6008381273522';
        fb_param.value = '8.00';
        fb_param.currency = 'USD';
        (function(){
          var fpw = document.createElement('script');
          fpw.async = true;
          fpw.src = '//connect.facebook.net/en_US/fp.js';
          var ref = document.getElementsByTagName('script')[0];
          ref.parentNode.insertBefore(fpw, ref);
        })();

        var google_conversion_id = 1007035867;
        var google_conversion_language = "en";
        var google_conversion_format = "3";
        var google_conversion_color = "ffffff";
        var google_conversion_label = "XVAiCIWg8AcQ28uY4AM";
        var google_conversion_value = 0;
        var google_remarketing_only = false;
        jq.getScript( "http://www.googleadservices.com/pagead/conversion.js" );

        /* Adroll HireMe Coversion */
        try {
            __adroll.record_user({'adroll_segments': 'posthireme_success'});
        } catch(err) {}
        /* End Adroll HireMe Coversion */
    }
    window.createInitialTransaction = function(amount, descr) {
        var hiremeAction = HiremeABTest.milestoneFormPercentageTest() ? "profilePage5A" : "profilePage5B";
        // track user comfirm to create milestone
        _ttref.push(['_trackEventNoCookie', 'HireMe', 'Initial_Milestone', hiremeAction, 'click']);
        var params = get_hireme_return_params();
        var submit_btn = jq('.btn-create-init-milestone');
        var cancel_btn = jq('.btn-cancel-init-milestone');
        var loading_img = cancel_btn.next();
        submit_btn.hide();
        cancel_btn.hide();
        loading_img.show();
        jq.post('/ajax/payment/onnewescrow.php',
            {
                username: params.seller_username,
                amount: amount,
                project: params.project_id,
                reason: "partly_payment",
                other_reason: descr,
                token: transferFundsToken,
                currency: params.currency_id
            },
            function(response) {
                if (response.status == "success") {
                    _t.push(['response_status', {
                        section: 'CreateHiremeInitMilestone',
                        status: 'Success'
                    }]);
                    jq('.container-milestone').remove();
                    params.url += "&ttref=HireMe_" + hiremeAction + "&hireme_initial_milestone_tracking=immediate";
                    window.location.href = params.url;
                } else {
                    if (response.needDeposit) {
                        _t.push(['response_status', {
                            section: 'CreateHiremeInitMilestone',
                            status: 'NeedDeposit'
                        }]);
                        window.location.href =
                            '/payments/deposit.php?amount=' + amount +
                            '&currency=' + params.currency_id +
                            '&refAction=hireme&id=' + params.project_id +
                            '&descr=' + descr +
                            '&username=' + params.seller_username +
                            '&bidderId=' + userId +
                            '&ttref=HireMe_' + hiremeAction +
                            '&hireme_initial_milestone_tracking=success_deposit_redirection' +
                            '&paymentItem=' + response.paymentItem + '&paymentDetail=true';
                        ;
                    } else {
                        _t.push(['response_status', {
                            section: 'CreateHiremeInitMilestone',
                            status: 'Fail',
                            msg: response.msg
                        }]);
                        submit_btn.show();
                        cancel_btn.show();
                        loading_img.hide();
                        alert(response.msg);
                    }
                }
            },
            'json'
        );
    }

    jq(document).ready(function() {


        jq('input[type="text"][maxlength],textarea[maxlength]').live('keyup', function() {
            jq(this).siblings('.description-length-count').find('span').text(jq(this).attr('maxlength') - jq(this).val().length);
        });

        jq('.milestone-learn-more').live('mouseover', function() {
            jq('.milestone-learn-more-hover').show();
        }).live('mouseout', function() {
            jq('.milestone-learn-more-hover').hide();
        });

        jq('.btn-create-init-milestone').live('click',function() {
            if (HiremeABTest.milestoneFormPercentageTest()) {
                var amount = jq('.container-milestone .milestone-amount').text();
            } else {
                var amount = jq('.container-milestone .milestone-amount').val();
            }
            var descr = jq.trim(jq('.container-milestone .milestone-descr').val());

            if (amount.length == 0) {
                alert('Amount Not Specified: Pease enter a Milestone Amount');
                return false;
            }
            if (!/^[0-9]{1,5}\.?[0-9]{0,2}$/.test(amount)) {
                alert('Invalid Amount: Only positive numbers accepted. e.g. 54 or 354.75');
                return false;
            }
            if (descr == '') {
                alert('Invalid Description: Please enter the description for this milestone');
                return false;
            }
            var params = get_hireme_return_params();

            _t.push(['user_action', {
                section: 'HiremeForm',
                label: 'CreateInitalMilestone',
                action: 'click',
                amount: parseFloat(amount),
                balance: parseFloat(params.balance),
                currency: params.currency_id
            }]);

            if (parseFloat(amount) > parseFloat(params.balance) && parseFloat(amount) * parseFloat(params.currency_exchangerate) > 1000) {
                if (confirm("Please confirm payment.\n\nYou do not have enough funds in this currency ("+ params.currency_code +") to make a payment.\n\nFor your convenience, we will automatically deduct the funds from your next highest currency balance. Otherwise, we will process the payment from your verified payment source.")) {
                    createInitialTransaction(amount, descr);
                }
            } else {
                createInitialTransaction(amount, descr);
            }
        });


        jq('.btn-cancel-init-milestone').live('click',function() {
            var params = get_hireme_return_params();
            if (!isLoggedInUserPaymentVerified) { //defined in profile_seo.php
                window.location.href = '/payments/verify.php?successRedirect=true&redirectType=project&redirectProjectId=' + params.project_id + '&ref=hireme&backToProjectPage=1';
                return;
            }

            _t.push(['user_action', {
                section: 'HiremeForm',
                label: 'CancelInitalMilestone',
                action: 'click'
            }]);

            var submit_btn = jq('.btn-create-init-milestone');
            var cancel_btn = jq('.btn-cancel-init-milestone');
            var loading_img = cancel_btn.next();
            submit_btn.hide();
            cancel_btn.hide();
            loading_img.show();
            window.location.href = params.url;
        });

        jq('input.milestone-descr[maxlength],textarea[maxlength]').live('keyup', function() {
            jq(this).siblings('.description-length-count').find('span').text(jq(this).attr('maxlength') - jq(this).val().length);
        });

        jq(function() {
            prepareHireMe();
            
            initialSkillSelect(typeof hireme_jobs!=='undefined'?hireme_jobs:null);
        });

        jq('#qh_descr').live('keyup', function() {
            limitText(jq(this), 4000);
            jq("#qh_descr_count").html(4000-jq('#qh_descr').val().length);
        }).live('keydown', function() {
            limitText(jq(this), 4000);
            jq("#qh_descr_count").html(4000-jq('#qh_descr').val().length);
        });

        jq('input.integer_only').live('keydown',function(e) {
            var txt = jq(this).val();
            if ( e.keyCode == 48 || e.keyCode == 96 ) {
                if(txt.length == 0) {
                    e.preventDefault();
                }
            }
            if ( e.keyCode == 46 || e.keyCode == 8 ) {
            } else {
                if (e.keyCode < 95) {
                    if (e.keyCode < 48 || e.keyCode > 57 ) {
                        e.preventDefault();
                    }
                } else {
                    if (e.keyCode < 96 || e.keyCode > 105 ) {
                        e.preventDefault();
                    }
                }
            }
        });

        jq('#qh_name').live('focusout', function() {
            if(qh_name_validate()){
                qh_name_duplicate_validate();
            }
        }).live('focusin', function() {
            discardErrorMessage({ element: '#qh_name' });
        });

        jq('#qh_descr').live('focusout', function() {
            qh_descr_validate();
        }).live('focusin', function() {
            discardErrorMessage({ element: '#qh_descr' });
        });

        jq('body').on('change', '.hireme-form #autoOpenToBidding', function(){
            qh_descr_validate();
        })

        jq('#qh_period').live('focusout', function() {
            qh_period_validate();
        }).live('focusin', function() {
            discardErrorMessage({ element: '#qh_period' });
        });
        // removeAbPosError can not support syntax like '.a .b'
        jq('#qh_fixed_budget').live('focusout', function() {
            qh_fixed_budget_validate();
        }).live('focusin', function() {
            discardErrorMessage({ element: '#qh_fixed_budget' });
        });
        jq('#qh_hourly_budget').live('focusout', function() {
            qh_hourly_budget_validate();
        }).live('focusin', function() {
            discardErrorMessage({ element: '#qh_hourly_budget' });
        });
        jq('#qh_hourly_commitment').live('focusout', function() {
            qh_hourly_commitment_validate();
        }).live('focusin', function() {
            discardErrorMessage({ element: '#qh_hourly_commitment' });
        });

        jq('input[type=radio][name=project-type]').live('click', function() {
            save_current_state();
            var budget_period = jq(this).val();
            var currency_id = hireMeProject.get("currency_id");

            hireMeProject.set({'budget_period':budget_period, 'min_budget':json_min_budget[budget_period][currency_id],'returnuser_loggedin':check_existing_user_passed});

            if (budget_period === 'hourly') {
                jq("#project_fix_fee_note").hide();
                jq("#project_hourly_fee_note").show();
            } else {
                jq("#project_fix_fee_note").show();
                jq("#project_hourly_fee_note").hide();
            }

            var currency_sign = jq(this).closest('.currency').find('option[value=1]').attr('title');
            jq(this).closest('.currency_sign').text(currency_sign);

            
                reRenderFeeNotes();
            

            restore_state();
            jq("input[type=radio][name=project-type][value=fixed]").trigger("toggleBudgetSuggestion");
        });


        jq('#tbx_username').live('focusin', function() {
            discardErrorMessage({ element: '#tbx_username' });
            //displayTooltip('#tbx_username', 'Please enter 3-16 alphanumeric characters [a-z 0-9] starting with a letter');
        }).live('focusout', function() {
            ajax_req_username = checkUsernameExistence(jq(this).val());
        }).live('keyup', function() {
            check_username_existence_passed = false;
        });
        jq('#username_txt').live('focusin', function() {
            discardErrorMessage({ element: '#username_txt' });
        }).live('focusout', function() {
            validate_existing_user();
        });
        jq('#passwd_txt').live('focusin', function() {
            jq('#passwd_txt').parent('.control-group').removeClass('error');
        }).live('focusout', function() {
            var eventArr = {event: 'Hire Me Page'};
            var localObj = new LocalTracking();
            localObj.push(eventArr);
            validate_existing_user();
        });

        jq('.fixed-project-container select.currency').live('change', function() {
            save_current_state();
            var currency_id = jq(this).val();
            var currency_sign = jq(this).find('option[value='+jq(this).val()+']').attr('title');
            var budget_period = hireMeProject.get("budget_period");
            getHiremeUpgradePrice(currency_id);
            hireMeProject.set({'currency_id':currency_id, 'currency_sign':currency_sign, 'min_budget':json_min_budget[budget_period][currency_id]});
            restore_state();
            jQuery(this).valid();
        });

        jq('.hourly-project-container select.currency').live('change', function() {
            save_current_state();
            var currency_id = jq(this).val();
            var currency_sign = jq(this).find('option[value='+jq(this).val()+']').attr('title');
            var budget_period = hireMeProject.get("budget_period");

            hireMeProject.set({'currency_id':currency_id, 'currency_sign':currency_sign, 'min_hourly_budget':json_min_budget[budget_period][currency_id]});
            if (budget_period === 'hourly') {
                jq("#project_fix_fee_note").hide();
                jq("#project_hourly_fee_note").show();
            } else {
                jq("#project_fix_fee_note").show();
                jq("#project_hourly_fee_note").hide();
            }

            restore_state();
            jQuery(this).valid();
        });
        /* for sigup form and login form */
        jq('input[type=radio][name=login_choice]').live('change', function() {
            var login_type =  jq('input[name="login_choice"]:checked').val();
            if (login_type == 'existing') {
                jq('#login-form').show();
                jq('#user-create-form').hide();
            } else {
                jq('#login-form').hide();
                jq('#user-create-form').show();
            }
        });
        jq('#tbx_email').live('focusin', function() {
            discardErrorMessage({ element: '#tbx_email' });
            //displayTooltip('#tbx_email', 'An email will be sent to this address for verification to complete the signup process');
        }).live('focusout', function() {
            ajax_req_email = checkEmailExistence(jq(this).val());
        }).live('keyup', function() {
            check_email_existence_passed = false;
        });

        jq('#tbx_username').live('focusin', function() {
            discardErrorMessage({ element: '#tbx_username' });
            //displayTooltip('#tbx_username', 'Please enter 3-16 alphanumeric characters [a-z 0-9] starting with a letter');
        }).live('focusout', function() {
            ajax_req_username = checkUsernameExistence(jq(this).val());
        }).live('keyup', function() {
            check_username_existence_passed = false;
        });
        jq('#username_txt').live('focusin', function() {
            discardErrorMessage({ element: '#username_txt' });
        }).live('focusout', function() {
            validate_existing_user();
        });
        jq('#passwd_txt').live('focusin', function() {
            discardErrorMessage({ element: '#username_txt' });
        }).live('focusout', function() {
            validate_existing_user();
        });

        jq('#passwd').live('focusin', function() {
            discardErrorMessage({ element: '#passwd' });
            discardErrorMessage({ element: '#passwd1' });
        }).live('focusout', function() {
            isPasswordErr();
        });
        jq('#passwd1').live('focusin', function() {
            discardErrorMessage({ element: '#passwd' });
            discardErrorMessage({ element: '#passwd1' });
        }).live('focusout', function() {
            isPasswordErr();
        });

        jq('.project-upgrades').live('click', function() {
            if (jq('input#featured').attr('checked') == 'checked') {
                jq('input#featured').removeAttr('checked');
            } else {
                jq('input#featured').attr('checked','checked');
            }
        });

        jq('input#featured').live('click', function(e) {
            e.stopPropagation();
        });

        /* this function should be override in the different pages, e.g. for achiving flip effect in contacts page
        jq('.hireme-form .btn-cancel').live('click', function() {
            jq('.hireme-form').parent().hide();
        });
        */

        jq('#ns_content').on('change', '[name=hours_option]', function() {
            var checkedOption = jq('[name=hours_option]:checked');
            if (checkedOption.val() == 'notSure') {
                jq('#qh_hourly_commitment').attr('disabled', true).val(1);
            } else {
                jq('#qh_hourly_commitment').removeAttr('disabled');
            }
            qh_hourly_commitment_validate();
        });

        jq('#ns_content').on('click', '.hours-option-text', function() {
            var forOption = jq(this).attr('for');
            jq('[name=hours_option][value=' + forOption + ']').trigger('click');
        });


        jq('.btn-hire-now').live('click', function() {

            if (validate()) {
                var trackLable = false;

                var newHref = getCookie('HIREME_HREF');
                if (trackLable == '' && newHref != '' && newHref === 'profile_hireme_click') {
                    _gaq.push(['_trackEvent', 'HireMe', 'profile_hirenow_success_click', 'profile_hireme_click']);

                } else {
                    _gaq.push(['_trackEvent', 'HireMe', 'profile_hirenow_success_click', '']);
                }

                /*new hireme tracking*/
                var additionalTrack = getCookie('HIREME_SOURCE_HREF');

                if (clickFromNavBar) {
                    _ttref.push(['_trackEvent', 'HireMe', 'Main', 'success_navbar']);
                } else {
                    _ttref.push(['_trackEvent', 'HireMe', 'Main', 'success']);
                }

                _t.push(['user_action', {
                    section: 'HiremeForm',
                    label: 'HireNowSuccess',
                    action: 'click'
                }]);

                /*end of new hireme tracking*/
                submit_form();
            } else {
                jq('body').animate({
                    scrollTop: jq('#hireme-form-container').offset().top
                });

                _t.push(['user_action', {
                    section: 'HiremeForm',
                    label: 'HireNowFail',
                    action: 'click'
                }]);
            }
        })

        //remove upload
        jq('.btn-remove-upload').live('click', function() {
            var li_obj = jq(this).parent().parent();
            var span_obj = jq(this).parent();
            li_obj.fadeOut(100);
            jq('#upload_files > option').each(function() {
                if(jq(this).html() == span_obj.children('b').html()) {
                    jq(this).remove();
                    return false;
                }
            });
            setTimeout(function() { li_obj.remove(); }, 1000);
        });

    });


});
</script>

<link rel="stylesheet" href="/static/css/blueimp-fileupload/jquery.fileupload.bootstrap.css">
<link rel="stylesheet" href="/static/css/blueimp-fileupload/jquery.fileupload-ui.css">

<script type='text/template' id='fileupload-template'>
    <div class="fileupload-buttonbar well">
    <div class="col-lg-7">
        <span class="btn fileinput-button">
            <i class="glyphicon glyphicon-plus"></i>
            <span> <%- T_('Add files...') %> </span>
            <input type="file" name='<%= param_name %>' multiple>
            <input type='hidden' name='fileset' class='upload-fileset'/>
        </span>
        <span class='muted' style='display:inline-block; margin-left: 10px; color:#888888;'> <%- T_('Drag and Drop multiple files here.') %> </span>
    </div>
</div>
<span id='fileupload-error' class='help-inline fileupload-error'></span>
<table role="presentation" class="table table-striped" style='width:100%'>
    <tbody class="files"></tbody>
</table>

</script>

<script id="template-upload" type="text/template">
    <tr class="template-upload fade <% if (error) {%> ui-state-error <% } %>">
    <td class="preview">
        <span class="fade"></span>
    </td>
    <td class="name"><%- shortName %></td>
    <td class="size"><%- size %></td>
    <% if (error) { %>
        <td class="error">Error:
            <% if(error === 'maxFileSize') { %>
                <%- T_('File is too big') %>
            <% } else if (error === 'minFileSize') { %>
                <%- T_('File is too small') %>
            <% } else if (error === 'acceptFileTypes') { %>
                <%- T_('Filetype not allowed') %>
            <% } else if (error === 'maxNumberOfFiles') { %>
                <%- T_('Max number of files exceeded') %>
            <% } else { %>
                <%- error %>
            <% } %>
        </td>
    <% } else { %>
        <td>
            <div class="progress progress-success progress-striped active" style='min-width: 20px'>
                <div class="bar"></div>
            </div>
        </td>
    <% } %>
    <td class="cancel">
        <button class="btn btn-warning">
            <i class="icon-ban-circle icon-white"></i>
            <span><%- T_('Cancel') %></span>
        </button>
    </td>
</tr>

</script>

<script id="template-download" type="text/template">
    <tr class="template-download <% if (error) {%> ui-state-error <% } %>" <% if (id) {%>fileid=<%-id%><% } %>>
    <% if (error) {%>
        <td class="error" colspan='4'>
            <span class='label label-important'>Error :</span>
            <%- name %>:
            <% if(error === 1) { %> File exceeds upload_max_filesize (php.ini directive)
            <% } else if(error === 2) { %>
                <%- T_('File exceeds MAX_FILE_SIZE (HTML form directive)') %>
            <% } else if(error === 3) { %>
                <%- T_('File was only partially uploaded') %>
            <% } else if(error === 4) { %>
                <%- T_('No File was uploaded') %>
            <% } else if(error === 5) { %>
                <%- T_('Missing a temporary folder') %>
            <% } else if(error === 6) { %>
                <%- T_('Failed to write file to disk') %>
            <% } else if(error === 7) { %>
                <%- T_('File upload stopped by extension') %>
            <% } else if(error === 'maxFileSize') { %>
                <%- T_('File is too big') %>
            <% } else if(error === 'minFileSize') { %>
                <%- T_('File is too small') %>
            <% } else if(error === 'acceptFileTypes') { %>
                <%- T_('Filetype not allowed') %>
            <% } else if(error === 'maxNumberOfFiles') { %>
                <%- T_('Max number of files exceeded') %>
            <% } else if(error === 'uploadedBytes') { %>
                <%- T_('Uploaded bytes exceed file size') %>
            <% } else if(error === 'emptyResult') { %>
                <%- T_('Empty file upload result') %>
            <% } else { %>
                <%- error %>
            <% } %>
        </td>
        <td class='delete'>
            <button class="btn btn-danger" data-type="" data-url="">
                <span><%- T_('Delete') %></span>
            </button>
        </td>
    <% } else { %>
        <td class="preview">
            <% if(thumbnail_url) { %>
                <a href="<%= url %>" target="_blank"><img src="<%= thumbnail_url %>"></a>
            <% } %>
        </td>
        <td class="name">
            <a <% if(url){%> href="<%=url%>" <%}%> <% if(thumbnail_url) { %> target="_blank"<% } %>> <%- shortName %> </a>
            <% if(filenameParamName) { %> <input type='hidden' name='<%- filenameParamName %>' value='<%- name %>'> <% } %>
        </td>
        <td class="size" colspan='2'> <%-size%> </td>
        <td class="delete">
            <button class="btn btn-danger" data-type="<%- delete_type %>" data-url="<%= delete_url %>" >
                <span><%- T_('Delete') %></span>
            </button>
        </td>
    <% } %>
</tr>

</script>


