    var skills = [{"id":"15","name":".NET","seo_url":"NET"},{"id":"169","name":"Adobe Dreamweaver","seo_url":"Dreamweaver"},{"id":"12","name":"Adobe Flash","seo_url":"Flash"},{"id":"92","name":"Algorithm","seo_url":"Algorithm"},{"id":"59","name":"Android","seo_url":"Android"},{"id":"107","name":"Animation","seo_url":"Animation"},{"id":"106","name":"C# Programming","seo_url":"C-Sharp-Programming"},{"id":"55","name":"Excel","seo_url":"Excel"},{"id":"335","name":"HTML","seo_url":"HTML.1"},{"id":"70","name":"Illustrator","seo_url":"Illustrator"},{"id":"307","name":"iPad","seo_url":"iPad"},{"id":"58","name":"iPhone","seo_url":"iPhone"},{"id":"7","name":"Java","seo_url":"Java"},{"id":"177","name":"Leads","seo_url":"Leads"},{"id":"32","name":"Logo Design","seo_url":"Logo-Design"},{"id":"44","name":"Mobile Phone","seo_url":"Mobile-Phone"},{"id":"305","name":"MySQL","seo_url":"MySQL"},{"id":"118","name":"Oracle","seo_url":"Oracle"},{"id":"57","name":"Photoshop","seo_url":"Photoshop"},{"id":"284","name":"Photoshop Design","seo_url":"Photoshop-Design"},{"id":"3","name":"PHP","seo_url":"PHP"},{"id":"68","name":"SQL","seo_url":"SQL"},{"id":"17","name":"Website Design","seo_url":"Website-Design"}];
    var base_update_url = "../users/profile";
    var skillsHtml = "<% _.each(skills, function(name) { %> <li><a class='exam_skills' job_id=<%= name.id %>><%= name.name %></a></li> <% }); %>";
    var skillsTemp = _.template(skillsHtml, skills);
    require([
        'jquery-wrapper',
        '/js/exam/recommendexam.js'
    ], function(jQuery, recommendExam) {

        jQuery(document).ready(function() {
            jQuery('#skills_div').html(skillsTemp);

            jQuery('.exam_skills').click(function() {
                var jobId = jQuery(this).attr('job_id');
                var jobName = jQuery(this).html();

                jQuery('#0_freelancerTable_skills').click();
                jQuery('#' + jobId + 'freelancerTable_skills').click();
                jQuery('.savePrefBtn').click();
                jQuery('#showFreelancer').click();
                jQuery('#rating_data_tab').click();

                _gaq.push(['_trackEvent', 'ProfilePage', 'Skills', 'Skill: ' + jobName]);
            });

            jQuery('#skills_edit_link').click(function() {
                jQuery('.profile_content').hide();
                jQuery('#edit-skills').fadeIn('slow');
                // load positions for the floating edit skills Nav (edit-skills.htm)
                loadPositionsPrep();
            });

            recommendExam.getRecommendedExam(getUserIdfromCookie(), false,
                '#exam-upsell',
                '#exam-upsell-name',
                '#exam-upsell-insig',
                '#exam-upsell-link',
                '#exam-upsell-text',
                'Exams_ProfileRecommend'
            );
        });
    });
</script>

