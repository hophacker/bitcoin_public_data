<script type="text/javascript">
  google.load("visualization", "1", {packages:["corechart"]});
  google.setOnLoadCallback(drawChart);
  function drawChart() {
     var options = {
      width: 194, height: 194,
      colors:['#FFB74C','#FFDB4C','#B0DA4C','#94CBE9','#FF7070','#FF8FBC'],
      legend:'none',
      pieSliceTextStyle:{color: 'black'},
      backgroundColor: { fill:'transparent' },
      tooltip: {textStyle: { color: 'black' }},
      chartArea:{width:"90%",height:"90%"}
    };

    
    var data = new google.visualization.DataTable();
    data.addColumn('string', 'Skills');
    data.addColumn('number', 'Past Reviews');
    data.addRows([
    ['Android', 20],['Mobile Phone', 13],['Java', 9],['iPhone', 7],['Photoshop', 2]
    ]);

    var chart = new google.visualization.PieChart(document.getElementById('chart_div'));
    chart.draw(data, options);
    

    
  }
</script>
