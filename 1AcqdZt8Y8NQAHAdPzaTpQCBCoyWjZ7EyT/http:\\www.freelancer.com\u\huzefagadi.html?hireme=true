    'affiliates/widget-tracking'
], function(portfolio, profile, invite, overview, ratings, widget_tracking) {

    jQuery(document).ready(function() {

        jQuery('#ns_profile-invite').click(function(e) {
            
            
                _gaq.push(['_trackEvent', 'ProfilePage', 'Sidebar', 'Invite Button (Logged Out)']);
            
        });

        
            window.preHiremePage = findDisplayedPage();
            showHireMe();
            window.hireme_jobs = ["59","58","44","7","57"];
        

        overview.initialize();
        invite.initialize();
        portfolio.initialize();
        profile.initialize();

        jQuery('#ns_profile-follow').click(function(e) {
            _gaq.push(['_trackEvent', 'ProfilePage', 'Sidebar', 'Follow Button']);
