<script type="text/javascript">
    jq('.badge_tab').live('click', function() {
        var id = jq(this).attr('id');
        var option = id.substring(0, id.indexOf("_tab"));
        var name = jq(this).children('a').html();

        if (option == 'all') {
            jq('.badge_box').show();
        } else {
            jq('.badge_box').hide();
            jq('#' + option + '_box').show();
        }
        jq('.badge_tab').removeClass('ns_current');
        jq(this).addClass('ns_current');

        _gaq.push(['_trackEvent', 'ProfilePage', 'Badges', name]);
    });

    jq(document).ready(function() {
        jq('.ns_badge-list').each(function() {
            if (jq(this).children().length == 0) {
                var badge = Object();
                badge.name = jq(this).attr('badge_name');
                jq(this).html(_.template(badgePlaceHolder, badge));
            }
        })

    });



    var badgePlaceHolder = "<div class='ns_empty ns_box ns_add' style='cursor: default'><p style='color: black;background: none;left: 150px;float: center;'> huzefagadi has not yet earned any <%= name %>.</p></div>"


</script>

    </div>
