<!---------------------------- STEP ONE FORM ---------------------------->
<script type="text/template" id="hireme-stepped-form-project-detail-template">

    <div class="ns_field projectNameField">
        <div class="ns_col">
            <label for="project-name"><%- HiremeABTest.showNewCopy() ? T_('What do you need?') : T_('Project name:') %>
            <span class="project-name-tip quick-hire-post-form-tip" data-trigger="hover" data-placement="right" data-title="Project Name" data-content="Your project name is important as it is what attracts freelancers to your project offer. You should clearly describe what you need in as few words as possible."> <img src="<%= cdn_url('/build/css/images/pages/post-project/question.png') %>"> </span> </label>
            <input id="qh_name" class="ns_full inline-error" type="text" name="name" value="<%- project_name %>">
        </div>
    </div>
    <div class="ns_field">
        <div class="ns_left">
            <label for="text-area"><%- HiremeABTest.showNewCopy() ? T_('Describe what you would like <[-username]> to do:', {username: window.username}) : T_('Describe your project in detail:') %>
            <span class="project-descr-tip quick-hire-post-form-tip" data-trigger="hover" data-placement="right" data-title="Project Description" data-content="The more detail you provide, the better chance you have in getting exactly what you are after in the shortest possible time period."> <img src="<%= cdn_url('/build/css/images/pages/post-project/question.png') %>"> </span> </label>
            <textarea id="qh_descr" class="ns_full ns_margin-10 inline-error" name="text-area" rows="6" cols="50"><%= descr %></textarea>
            <span class="ns_right ns_highlight"><span id="qh_descr_count"><% if (descr) { %><%= 4000-descr.length %><% } else { %>4000<% } %></span> Characters left</span>
        </div>
    </div>
        
    <div id="fileUploadView">
        <!-- The file upload form used as target for the file upload widget -->
    </div>

    <div class="ns_field">
        <label class="ns_margin-10"><%- HiremeABTest.showNewCopy() ? T_('Which of these skills are relevant?') : T_("Which of the freelancer's skills are required for this project?") %></label>
        <div class="ns_col">
            <select multiple class="ns_full" id="skill_select">
                <%    if ( all_skills ) {
                for ( var i=0; i < all_skills.length; i++ ) {
                %>
                <option job_id="<%= all_skills[i].id %>" value="<%= all_skills[i].id %>"><%= all_skills[i].name %></option>
                <%         }
                }
                %>
            </select>
            <div>
                <span id="skill_select_count"></span>
                <br>
                <span id="qh_skill_err"></span>
            </div>
        </div>
    </div>
    
    <div class="ns_field">
      <label class="checkbox">
        <input type="checkbox" id="autoOpenToBidding" name="autoOpenToBidding" value="true" <%- autoOpenToBidding ? 'checked' : ''%>/>
        <div class="chxboxDesc">
            <strong><%- T_('Get free bids from freelancers if <[-freelancer_name]> does not accept.', {freelancer_name: window.username})%></strong>
            <span class="quick-hire-post-form-tip" id="autoOpenToBidding-tip" data-title="<%- T_('Get free bids') %>" data-content="<%- T_('Receive bids from freelancers if <[-username]> does not accept. Your project description will be made public when this occurs.',{'username': window.username})%>">
              <img src="<%= cdn_url('/build/css/images/pages/post-project/question.png') %>">
            </span>
        </div>
      </label>
    </div>
    
    <div>
        <a id="nextStep" class="ns_btn ns_blue">Continue</a>
    </div>
</script>

<!---------------------------- STEP TWO FORM ---------------------------->
<script type="text/template" id="hireme-stepped-form-project-budget-template">

    <fieldset>
        <div class="ns_field">
            <ul class="ns_horizontal">
                <li>
                    <label>
                        <input class="project-type-stepped" type="radio" name="project-type-stepped" value="fixed" <% if ( budget_period == 'fixed' ) { %>checked<% }%>>
                        <span class="ns_bold">Fixed Price</span></label>
                </li>
                <li>
                    <label>
                        <input class="project-type-stepped" type="radio" name="project-type-stepped" value="hourly" <% if ( budget_period == 'hourly' ) { %>checked<% } %>>
