    <li><a class="step <%-step==2?'active':''%>" data-step="2"><strong>2</strong> <%- isNotLogin ? T_('Duration') : T_('Submit') %></a></li>
    <% if(isNotLogin) { %>
    <li><a class="step <%-step==3?'active':''%>" data-step="3"><strong>3</strong> Submit</a></li>
    <% } %>
</ul>
</script>

