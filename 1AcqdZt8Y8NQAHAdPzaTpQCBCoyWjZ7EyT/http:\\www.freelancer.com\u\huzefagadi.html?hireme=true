<script>
    require([
        'jquery-wrapper',
        'hireme/hiremeSteppedForm',
        'fileupload-main'
    ], function(jQuery, hireme, fileUploadLib) {
        window.fileuploadStepped = fileUploadLib.init({
            container: '#steppedUploadFileView',
            blueimpeFileupload: {
                url: '/buyers/postproject-upload.php?fileset=',
                paramName: 'upload[]',
                maxFileSize: 2000000,
                autoUpload: true
            }
        });
    });
</script>


