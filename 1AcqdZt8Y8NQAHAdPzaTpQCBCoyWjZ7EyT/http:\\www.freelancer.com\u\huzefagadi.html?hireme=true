    <script type="text/javascript">
    var _adq = _adq || [];

    var Ads = {
        config: {
            base_url: '//s.fl-ads.com',
            easyXDM: {
                local: '/js/easyXDM/name.html', // don't use CDN for this
                swf: 'https://cdn2.f-cdn.com/js/easyXDM/easyxdm.swf?v=33978b8e161aaec7dfe12704d80914b4&amp;m=2'
            }
        },
        env: 'production'
    };

    (function(document, location) {
        var script = document.createElement('script');
        script.src = location.protocol + '//s.fl-ads.com/js/serve/main/parent/adq.js';
        script.async = true;
        script.type = 'text/javascript';

        var position = document.getElementsByTagName('script')[0];
        position.parentNode.insertBefore(script, position);
    }(document, window.location));
</script>

    

    

    

    <!-- tracking stuffs starts -->
    
    <!-- Google Code for Remarketing Tag -->
<script type="text/javascript">

    if ((typeof window.usertype != 'undefined') && (window.usertype != '')) {
       loadGARemarketingPixel(window.usertype);
    }

    function loadGARemarketingPixel(usertype) {
        if (!document.getElementById('remarketingPixel')) {
            var google_tag_params = google_tag_params || {};
            var trackUrl = "https://www.googleadservices.com/pagead/conversion/viewthroughconversion/1015002772/";
            var params =  "?value=0&guid=ON&script=0&remarketing_only=true";
            params = params + '&usertype=' + usertype;
            var img = document.createElement("img");
            img.id = 'remarketingPixel'
            img.src = trackUrl + params;
            document.body.appendChild(img);
        }
    }
</script>
<noscript>
    <div style="display:inline;">
        <img height="1" width="1" style="border-style:none;" alt="" src="//googleads.g.doubleclick.net/pagead/viewthroughconversion/1015002772/?value=0&amp;guid=ON&amp;script=0"/>
    </div>
</noscript>

    <!-- tracking stuffs ends -->

