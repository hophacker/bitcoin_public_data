                    </div>
                    <div class="insufficientFunds align-l">
                        <div class="alert margin-b0 convert hide">
                            <h4 class="margin-b10 align-l" style="color:#c09853;font-weight:bold">
                                <%= T_('You do not have sufficient funds to complete this transaction (<[= currencySign]><span class="convertAmount"><[- convertAmount]></span> <[- currencyCode]> required)', {'currencySign': currencySign, 'convertAmount': (typeof prepaidMilestone != 'undefined') && prepaidMilestone ? convertAmount - prepaidMilestone.amount : convertAmount, 'currencyCode': currencyCode}) %>
                            </h4>
                            <p>
                                <%= T_('For your convenience, we will attempt to deduct the remaining amount from your other currency balances, then if required, attempt to process the payment from your verified payment sources.') %>
                                <%= T_('Otherwise, you can deposit funds via the <a href="<[=link]>">deposit page</a>.', {'link': '/payments/deposit.php'}) %>
                            </p>
                        </div>
                        <div class="alert margin-b0 deposit hide">
                            <h4 class="margin-b10 align-l" style="color:#c09853;font-weight:bold">
                                <%= T_('You do not have sufficient funds for this transaction (<[= currencySign]><span class="convertAmount"><[- convertAmount]></span> <[- currencyCode]> required)', {'currencySign': currencySign, 'convertAmount': (typeof prepaidMilestone != 'undefined' && prepaidMilestone) ? convertAmount - prepaidMilestone.amount : convertAmount, 'currencyCode': currencyCode }) %>
                            </h4>
                            <p>
                                <%= T_('Please <a href="<[= link]>">deposit funds</a> and try again.', {'link': '/payments/deposit.php'}) %>
                            </p>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-primary btn-modal-create-initial-milestone" action="award">
                        <%= T_("Create Milestone") %>
                    </button>
                    <button class="btn btn-modal-cancel-initial-milestone">
                        <%= T_('No Thanks') %>
                    </button>
                </div>
            </div>
            <% } %>
        </div>
        <% } %>
    <% } else if (is_pending) { %>
        <% if (!project.isQuickHireProject) { %>
            <button class="btn btn-mini bold openRevokeConfirmation margin-t10" ><%= T_('Revoke Award')%></button>
            <div class="well silver padding-5 align-c revokeConfirmation" style="display:none">
                <div class="margin-b5 margin-0"><%= T_('Revoke Now?')%></div>
                <button class="btn btn-primary bold bidAction" action="revoke"><%= T_('Yes')%></button>
                <button class="btn bold cancelRevoke"><%= T_('Cancel')%></button>
            </div>
        <% } %>
    <% } %>
</div>
<div class="bid-more-actions <%- (typeof showNDA != 'undefined' && showNDA) ? 'smaller' : 'small' %>">
    <% if(typeof shortlist != "undefined" && showShortlist && !is_selected) { %>
        <% if (shortlist) { %>
        <i class="icon-shortlisted"></i>
        <a href="#" class="link-shortlisted shortlist-bid" onclick="return false" data-action="unshortlist">
                <%= T_('Shortlisted') %>
        </a>
        <%} else { %>
        <i class="icon-shortlist"></i>
        <a href="#" class="link-shortlist shortlist-bid" onclick="return false" data-action="shortlist">
                <%= T_('Shortlist') %>
        </a>
        <% } %>
    <% } else { %>
        <a href="#" onclick="return false" class="addBidNoteLink" style="display:none"><i class="icon-grey icon-pencil"></i> <%- T_('Add Note')%></a>
    <% } %>
    <% if (typeof showNDA != 'undefined' && showNDA) { %>
        <a href="/NDA/NDA-print-contract.php?project_id=<%= project.id %>&users_id=<%= bidder_id %>" class="viewNDALink" target="_blank">
            <i class="icon-grey icon-file"></i>
            <%- T_('View NDA')%>
        </a>
    <% } %>
</div>

