    <script src="//d2werhn82xczly.cloudfront.net/20140328/main.min.js" type="text/javascript"></script>
<script type="text/javascript">
        try {
            _t = new Tracking(
              '//t.freelancer.com/1px.gif',
              flns.config.cookie_domain,
              {'user_id': getCookie(flns.config.cookie_base + '_USER_ID')}
            ).init(_t);
            _lt = new LocalTracking().init(_lt);
        } catch (err) {
            if (console) { console.log(err); }
        }

        _t.push(['page_view']);
        if (typeof page_start_time === 'undefined') {
        // doing nothing
        } else {
            var rendering_timeline = {
                'start': page_start_time,
                'dom_ready': (new Date),
                'onload': (new Date)
            };

            jQuery.ready(function () {
                rendering_timeline.dom_ready = new Date;
            });

            jQuery(window).load(function () {
                rendering_timeline.onload = new Date;
                var p = {
                    'url': ('' + window.location),
                    'start': rendering_timeline.start.getTime(),
                    'dom_ready': rendering_timeline.dom_ready.getTime(),
                    'onload': rendering_timeline.onload.getTime()
                };
                _t.push(['page_rendering', p]);
                /* Here we push to the dataLayer, if it is defined.
                 The dataLayer is a part of google tag manager, and it's where we can push
                 events and the things we're interested in.

                 In this case we're interested in the page timing, so we push those timings into
                 the data layer, and then a "page load finished" event so we know when the page
                 load is finished.
                  */
                if (typeof dataLayer != undefined) {
                    dataLayer.push({'page_timing_ms.dom_ready': rendering_timeline.dom_ready.getTime() - rendering_timeline.start.getTime()});
                    dataLayer.push({'page_timing_ms.onload':    rendering_timeline.onload.getTime() - rendering_timeline.start.getTime()});
                    dataLayer.push({'event': 'page load finished'});
                }
            });

            if (Math.random() < 0.01) {
                var start_latency = new Date().getTime();
                jQuery.ajax({
                    url: '/pong.php',
                    success: function () {
                        _t.push(['latency', {'latency': (new Date().getTime() - start_latency)}]);
                    }
                });
            }
        }

        if (typeof window.performance !== 'undefined') {
            jQuery(window).load(function() {

                try {
                    var pt = window.performance.timing;

                    _t.push(['page_performance_timings', {
                        'navigationStart' : pt.navigationStart,
                        'unloadEventStart' : pt.unloadEventStart,
                        'unloadEventEnd' : pt.unloadEventEnd,
                        'redirectStart' : pt.redirectStart,
                        'redirectEnd' : pt.redirectEnd,
                        'fetchStart' : pt.fetchStart,
                        'domainLookupStart' : pt.domainLookupStart,
                        'domainLookupEnd' : pt.domainLookupEnd,
                        'connectStart' : pt.connectStart,
                        'connectEnd' : pt.connectEnd,
                        'responseStart' : pt.responseStart,
                        'responseEnd' : pt.responseEnd,
                        'domLoading' : pt.domLoading,
                        'domInteractive' : pt.domInteractive,
                        'domContentLoadedEventStart' : pt.domContentLoadedEventStart,
                        'domContentLoadedEventEnd' : pt.domContentLoadedEventEnd,
                        'domComplete' : pt.domComplete,
                        'loadEventStart' : pt.loadEventStart,
                        'loadEventEnd' : Date.now()
                    }]);
                } catch (err) {
                    // quiet handle
                }
            });
        }
</script>
