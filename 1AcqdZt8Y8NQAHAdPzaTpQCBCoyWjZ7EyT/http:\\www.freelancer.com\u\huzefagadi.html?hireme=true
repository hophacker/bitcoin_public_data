<script type="text/javascript" src="//secure.addthis.com/js/250/addthis_widget.js#async=1&pub=getafreelancer"></script>
<script type="text/javascript">
    if (typeof addthis != 'undefined' && addthis) {
        addthis.init();
    }

    // Tab Pages AutoLoad - check if directed from email link content, then open appropriate tab page.

    var getTrackingMedium = getQueryStringValueByName('utm_medium');
    var getTrackingSource = getQueryStringValueByName('utm_source');
    var refprojectId = getQueryStringValueByName('ref_project_id');
    var pvpReviewLink = getQueryStringValueByName('pvpReviewLink');
    var redirectToPage = getQueryStringValueByName('page');

    var refProjIds;
    var refProjJobList;

    //checks if parameters exist and get projectjobs data for auto filtering reviews from related projects

    if ((!isOwner) && ((typeof getTrackingMedium != 'undefined') && (typeof getTrackingSource != 'undefined')) || (typeof pvpReviewLink != '')) {
        if (((getTrackingMedium == 'email') && (getTrackingSource == 'feedback')) || (typeof pvpReviewLink != '')) {
            jq(document).ready(function(){
                jq.ajax({
                    type: 'post',
                    url: base_url + '/ajax/project/getProjectJobs.php',
                    data: {project_id: refprojectId},
                    dataType: 'json',
                    success: function(resp) {
                        if (resp.status == 'success') {

                            refProjIds = JSON.parse(resp.jobIds);
                            refProjJobList = JSON.parse(resp.jobNames);

                            if (((typeof refProjIds != 'undefined') && (refProjIds.length > 0)) && (redirectToPage == '')) {
                                jq('.ns_profile #rating_data_tab').trigger('click');
                                showFreelancerView();
                                filterOnSkill(refProjIds);
                                filterOnType('Fixed|Hourly');
                                filterOnState('Completed');
                            }
                        }
                    },
                    complete: function() {
                        jq('#skill_list').html(refProjJobList);
                        jq('#project_list').html('<li>Fixed</li><li>Hourly</li><li>Completed</li>');
                        jq('#selected_skills').show();
                    }
                });
             });
        }
    }

</script>

