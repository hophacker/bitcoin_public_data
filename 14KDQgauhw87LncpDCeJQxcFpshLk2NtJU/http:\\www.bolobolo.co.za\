			function fLoadEvent(id,W,H){
				Shadowbox.open({
					content:    'event'+id+'.html',
					player:     "iframe",
					title:      "",
					height:     H,
					width:      W
				});
			}
			
			function fLoadBook(id,W,H){
				Shadowbox.open({
					content:    'book'+id+'.html',
					player:     "iframe",
					title:      "",
					height:     H,
					width:      W
				});
			}
			
