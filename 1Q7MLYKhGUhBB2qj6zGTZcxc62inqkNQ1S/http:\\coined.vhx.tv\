      $(document).ready(function() {
        // ------------------------------------------------------------------------
  var VHXJS = VHXJS || {};
  
  /* theme js object
  ---------------------------------------------*/
  VHXJS = {
  
  	buyOptionsList: $(".buy-block ul"),
    
  	/* initiate
    ---------------------------------------------*/
   
    init: function() {
     // hide trailer buttons if no link
  		$("a.package-trailer-btn[href='']").hide();
      
      this._moveiFrame();
      this._setupVignette();
      this._bindEvents();
      this._setupBuyButton();
    },
    
    _moveiFrame: function(){
    	if (screen.width < 500){
  	    $("#vhx-iframe").hide();
    	} else {
  		  $("#vhx-iframe").css("top","38px");
      }
    },
    
    _bindEvents: function() {
      $('.nav-pri ul li').find('a').click(function(){
          var $href = $(this).attr('href');
          var $anchor = $($href).offset();
          if ($href == "#buy") {
          	$(this).toggleClass("active");
          	VHXJS.__closeNavSubmenus();
        		VHXJS.buyOptionsList.stop().slideToggle();
          } else {
          	$('body').animate({ scrollTop: $anchor.top-20 });
          }
          return false;
      });
      
      $('.feature-options .primary').click(function(){
      	if ($(this).attr('href') == "#") {
      		var $anchor = $("#package-area").offset().top - 20;
      		$('body').animate({ scrollTop: $anchor });
        }
      });
      
      $(".redeemer, .redeemer a, #redemption .close").click(function(){
      $("#redemption").toggleClass("active");
        return false;
      });
      $("#redemption .formbutton").click(function(){
        if ($("#redemption .formtextfield").val()){
          window.location = "/buy/?code="+$("#redemption .formtextfield").val();
        } else {
          $("#redemption").addClass("error");
        }
      });
    },
    
    _setupBuyButton: function(){
    	// monitor buy switch
      /*$('.feature-options a.primary').on('mouseenter',function() {
        VHXJS.__closeNavSubmenus();
        VHXJS.buyOptionsList.stop().slideToggle();
      });*/
      $('.buy-block ul li').on('mouseenter', function() {
        VHXJS.__closeNavSubmenus();
        $(this).addClass("active").children('.details').stop().slideDown();
      });
      $('.buy-block ul li').on('mouseleave', function() {
        $(this).removeClass("active");
      });  
    },
    
    __closeNavSubmenus: function() {
        $('.buy-block ul li .details').stop().slideUp();
    },
    
    setUpdates: function() {
      if ($('#twitter').length < 1 || $('#tumblr').length < 1) { //all content in one column
        $(".newsletter label").css({
          "text-align": "center",
          "display": "block"
        });
        
        $(".newsletter").css({
          "width": "280px",
          "margin": "0 auto",
          "padding-top": "20px",
          "padding-bottom": "10px"
        });
        
        $(".newstablet").css({
          "display": "block"
        });
        
        $(".newsnormal").css({
          "display": "none"
        });
        
        //restrict blog/twitter width and center 
        if ($(window).width() > 768) {
          $(".updates .wide").css({
            "width": "100%"
          });
          
          $(".updates .skinny").css({
            "width": "100%"
          });
          
          $(".updates .contained").css({
            "width": "700px"
          });
       } else {
          $(".updates .wide").css({
            "width": "auto"
          });
          
          $(".updates .skinny").css({
            "width": "auto"
          });
          
          $(".updates .contained").css({
            "width": "auto"
          });
        }
      }
    },
    
    setPackages: function() {
      if ($(".package").length > 0){ 
        if ($(window).width() < 750) {
          $(".packages .visual").css("width", "100%");
          $(".packages .desc").css("width", "100%");
          $(".packages li").removeClass("pair");
          $(".packages .desc").css({"top": "-90px"});
          if ($(".package").length == 3){ 
            $(".packages .visual").css("width", "100%");
            $(".packages li").removeClass("triple");
            $(".packages .desc").css({"font-size": "14px", "line-height": "20px", "width": "100%", "top": "-90px"});
            $(".packages .package-trailer-btn").css({"margin-top": "40px"});
            $(".packages").css("max-width", "auto");
            $(".packages .package-trailer-btn").css({"margin-left": "15px", "margin-top": "28px"});
            $(".packages .package-btn").css({"margin-top": "15px"});
          }
          if ($(window).width() < 600) {
            $(".packages .desc").css({"top": "-40px"});
            $(".packages .package-trailer-btn").css({"margin-left": "15px", "margin-top": "25px"});
          }
          
          if ($(window).width() <= 420) {
            $(".packages .desc").css({"top": "-40px"});
            $(".packages .package-trailer-btn").css({"margin-left": "0px", "margin-top": "15px"});
          }
        } else {
          $(".packages .desc").css({"top": "-70px"});
          if ($(".package").length == 2 || $(".package").length > 3){ //split in half
            $(".packages .visual").css("width", "100%");
            $(".packages .desc").css("width", "100%");
            $(".packages li").removeClass("single");
            $(".packages li").addClass("pair");
            $(".packages").css("max-width", "884px");
          } else if ($(".package").length == 3){ 
            //keep on one line if not tablet
            $(".packages .visual").css("width", "100%");
            $(".packages li").addClass("triple");
            $(".packages .desc").css({"font-size": "14px", "line-height": "20px", "width": "100%", "top": "-40px"});
            $(".packages").css("max-width", "960px");
            $(".packages .package-btn").css({"margin-top": "40px"});
            $(".packages .package-trailer-btn").css({"margin-left": "0px"});
          } else if ($(".package").length == 1) {
            $(".packages").css("max-width", "484px");
          }
        }
      }
      
      //align package buttons
      if ($(window).width() > 749) {
        $(".packages .desc .copy-1").css({"height": "auto"});	
        $(".packages .desc .title-2").css({"height": "auto"});
        var maxTitleH = Math.max.apply(null, $(".packages li .title-2").map(function() {
          return $(this).height();
          }));
        
        $(".packages .desc .title-2").css({"height": maxTitleH});
        
        var maxCopyH = Math.max.apply(null, $(".packages li .copy-1").map(function() {
          return $(this).height();
          }));
        $(".packages .desc .copy-1").css({"height": maxCopyH});
       } else {
          $(".packages .desc .copy-1").css({"height": "auto"});	
          $(".packages .desc .title-2").css({"height": "auto"});
        }
      
      //set packages height
      var newHeight = $(".packages ul.contained").height();
      if ($(window).width() >= 750) {
        newHeight = newHeight - 50;
      }
      if ($(window).width() <= 600) {
        newHeight = newHeight + 80; 
      }
      $(".packages").css({"height": newHeight, "margin-bottom": "30px"});   
    },
    
   	// utility function
    componentToHex: function(c) {
      var hex = c.toString(16);
      return hex.length == 1 ? "0" + hex : hex;
  	}, 
    // utility function
    rgbToHex: function(r, g, b) {
      return "#" + VHXJS.componentToHex(r) + VHXJS.componentToHex(g) + VHXJS.componentToHex(b);
  	},  
    // 
    _setupVignette: function() {
  		// only do this if we actually have packages to show
  		if ($(".content .package").length > 0){
      	// get the bg color value from .content
      	var colour = $(".packages").css("background-color");    
        colour = colour.replace("rgb(","");
        colour = colour.replace(")","");
        colour = colour.replace(/ /g,"");
        colourArr = colour.split(",");
        // converts the bg color from browser rgb to usable hex
        var bgHex = VHXJS.rgbToHex(parseInt(colourArr[0]),parseInt(colourArr[1]),parseInt(colourArr[2]));
        
        // $('.test').css({ 'background': '-webkit-gradient(linear,left top,left bottom,from(#f4f4f4),to(#FFFFFF))' });
        // build the before gradient
        var prefixes = new Array("-moz-linear-","-webkit-linear-","-o-linear-","-ms-linear-","linear-");
        var beforestr, afterstr = "";
        var beforedir = "top";
        for(var i =0;i<=prefixes.length-1;i++){
          if (i == prefixes.length-1){
            beforedir = "to bottom";
          }
          // before = bottom 
          gradientstr = prefixes[i]+"gradient("+beforedir+",  rgba("+colourArr[0]+","+colourArr[1]+","+colourArr[2]+",0) 0%, rgba("+colourArr[0]+","+colourArr[1]+","+colourArr[2]+",1) 100%)";
          
          $(".package .visual .gradient").css("background",gradientstr);
        }
   
      } 
    }
  };
  
  (function(window,document,$,VHXJS) {
  	VHXJS.init();
  })(window,document,jQuery,VHXJS);
  
  
  /* ------------------------------------------------------------------------ */
  /* RESIZING */
  /* ------------------------------------------------------------------------ */
  $(window).resize(function() {
    VHXJS.setPackages();
    VHXJS.setUpdates();
  });
  
  $(window).on("load", function() { $(window).trigger("resize"); });
  
  ///////////-----------------------
  
  /* ------------------------------------------------------------------------ */
  /* CONTROL IPAD SWITCHING FROM PORTRAIT TO LANDSCAPE WITHOUT ZOOMING */
  /* ------------------------------------------------------------------------ */
  if (navigator.userAgent.match(/iPhone/i) || navigator.userAgent.match(/iPad/i)) {
      var viewportmeta = document.querySelector('meta[name="viewport"]');
      if (viewportmeta) {
          viewportmeta.content = 'width=device-width, minimum-scale=1.0, maximum-scale=1.0, initial-scale=1.0';
          document.body.addEventListener('gesturestart', function () {
              viewportmeta.content = 'width=device-width, minimum-scale=0.25, maximum-scale=1.6';
          }, false);
      }
  }
  
  /* ------------------------------------------------------------------------ */
  /* TUMBLR */
  /* ------------------------------------------------------------------------ */
  // ------------------------------------------------------------------------
  // RETRIEVES TUMBLR DATA
  if ($('#tumblr').length > 0) {
    getTumblog($("#tumblr").data("tumblr"), "#tumblr");
  } 
      
  function getTumblog(username, target){
  	
  	var tumblrkey = "mhg6b9xFS5pGZr9inve9sSsa0Zl2zy4ojb3K7Ta3aWswFqqSU8";
    var tumblruser = username + ".tumblr.com";
    var endpoint = "http://api.tumblr.com/v2/blog/" + tumblruser + "/posts/";
  
    var data = {
      api_key: tumblrkey
    };
    
    setTimeout(function() {
      $.ajax({
        type: "GET",
        url: endpoint,
        dataType: "jsonp",
        data: data
      })
      .done(function(returnData) {
        processTumblog(returnData, username, target);
      });
      
    }, 500);
  }
  // ------------------------------------------------------------------------
  // PARSES TUMBLR DATA INTO ARRAY
  function processTumblog(data, username, target){
  
  	var limit = parseInt($("#tumblr").data("tumblr-limit"));
    var usernamesafe = username.replace(/-/g,'');
  	var html = '<div class="rssBody" id="tumblr_'+usernamesafe+'"><ul id="blog-entries"></ul></div>';
    
    if (data.meta.status == 200) {
        var feed = {
          url: 'http://' + username + '.tumblr.com',
          posts: []
        };
  
        $(data.response.posts).each(function(i, post) {
  
          switch (post.type){
            case "text":
              feed.posts.push({
                text: {
                  title: post.title,
                  body: post.body
                }
              });
              break;
  
            case "photo":
              var photos = [];
              $(post.photos).each(function(i, obj){
                photos.push({
                  photo_url: obj.alt_sizes[1].url // 500px wide
                })
              });
  
              feed.posts.push({
                photo: {
                  caption: post.caption,
                  photos: photos
                }
              });
              break;
  
            case "quote":
              feed.posts.push({
                quote: {
                  type: post.type,
                  tags: post.tags,
                  text: post.text,
                  source: post.source
                }
              });
              break;
  
            case "link":
              feed.posts.push({
                link: {
                  linkURL: post.url,
                  title: post.title,
                  description: post.description
                }
              });
              break;
  
            case "chat":
              var chat_log = post.body.split("\n");
              feed.posts.push({
                chat: {
                  title: post.title,
                  body: post.body,
                  chat: chat_log,
                  dialogue: post.dialogue
                }
              });
              break;
  
            case "audio":
              feed.posts.push({
                audio: {
                  source_title: post.source_title,
                  source_url: post.source_url,
                  id3_title: post.id3_title,
                  caption: post.caption,
                  audio_player: post.player,
                  audio_plays: post.plays,
                  artist: post.artist,
                  album: post.album,
                  track_name: post.track_name,
                  track_num: post.track_num,
                  year: post.year
                }
              });
              break;
  
            case "video":
              feed.posts.push({
                video: {
                  source_title: post.source_title,
                  caption: post.caption,
                  video_embed: post.player[2].embed_code
                }
              });
              break;
  
            case "answer":
              feed.posts.push({
                answer: {
                  asking_name: post.asking_name,
                  asking_url: post.asking_url,
                  question: post.question,
                  answer: post.answer
                }
              });
              break;
  					
  
            default:
              feed.posts.push({
                title: post.title,
                body: post.body
              });
          } // end switch
  			       
          feed.posts[feed.posts.length - 1].date = post.date.substr(0,10);
          feed.posts[feed.posts.length - 1].tags = post.tags;
          feed.posts[feed.posts.length - 1].timestamp = post.timestamp;
          feed.posts[feed.posts.length - 1].url = post.post_url;
          feed.posts[feed.posts.length - 1].type = post.type;
  				feed.posts[feed.posts.length - 1].note_count = post.note_count;
          
        }); // end each
        
       if (limit > 0) {
  	     feed.posts = feed.posts.slice(0, limit);
       }
       $("#tumblr").html(createTumblrHTML(feed, username));
       
  	} // end if
  } // end function
  // ------------------------------------------------------------------------
  // PARSES DATA FEED INTO STANDARD HTML
  function createTumblrHTML(data, username){
  	
    var thehtml ="";
    
    $(data.posts).each(function(i, post) {
          
      switch (post.type){
      	case "text":
        	 thehtml +='<div class="entry text">';
           thehtml +='   <h2><a href="'+post.url+'">'+post.text.title+'</a></h2>';
           thehtml +=			post.text.body;
           if (post.tags.length >0){  thehtml += createTumblrTags(post.tags);	 }
           thehtml +='   <div class="timestamp"><a href="'+post.url+'"><i class="icon-time"></i> Posted '+post.date+' <i class="icon-heart"></i> '+post.note_count+'</a></div>'; 
           thehtml +=' </div>';
        	break;
      
        case "photo":
         	 
           thehtml +='<div class="entry photo">';
           thehtml += '   <div class="photos">';
  				 $(post.photo.photos).each(function(j, thephoto){
           	thehtml += ' <img src="'+thephoto.photo_url+'" />';
           });
           thehtml += '  </div>';
           thehtml += 	 post.photo.caption;
           if (post.tags.length >0){  thehtml += createTumblrTags(post.tags,username);	 }
           thehtml += '  <div class="timestamp"><a href="'+post.url+'"><i class="icon-time"></i> Posted '+post.date+' <i class="icon-heart"></i> '+post.note_count+'</a></div>';
           thehtml +=' </div>';
          break;
        
        
   			case "quote":
        	 thehtml +='<div class="entry quote">';
           thehtml +='   <h2>'+post.quote.text+'</h2>';
           thehtml +='   <p class="source">'+post.quote.source+'</p>';
           if (post.tags.length >0){  thehtml += createTumblrTags(post.tags,username);	 }
           thehtml += '  <div class="timestamp"><a href="'+post.url+'"><i class="icon-time"></i> Posted '+post.date+' <i class="icon-heart"></i> '+post.note_count+'</a></div>';
           thehtml +=' </div>';
        	break;
          
        case "link":
        	thehtml +='<div class="entry link">';
          thehtml +='    <h2><a href="'+post.link.linkURL+'">'+post.link.title+'</a></h2>';
          thehtml +=		post.link.description;
          if (post.tags.length >0){  thehtml += createTumblrTags(post.tags,username);	 }
          thehtml += '  <div class="timestamp"><a href="'+post.url+'"><i class="icon-time"></i> Posted '+post.date+' <i class="icon-heart"></i> '+post.note_count+'</a></div>';
  				thehtml +=' </div>';        
        	break;
        
        case "chat":
        	thehtml +=' <div class="entry chat">';
          thehtml +='    <h2><a href="'+post.url+'">'+post.chat.title+'</a></h2>';
          thehtml +='    <ul>';
          $(post.chat.chat).each(function(k, chatter){
          thehtml +='        <li>'+chatter+'</li>';
          });
          thehtml +='    </ul>';
          if (post.tags.length >0){  thehtml += createTumblrTags(post.tags,username);	 }
          thehtml += '  <div class="timestamp"><a href="'+post.url+'"><i class="icon-time"></i> Posted '+post.date+' <i class="icon-heart"></i> '+post.note_count+'</a></div>';  
          thehtml +=' </div>';
        	break;
       	
        case "audio":
        	thehtml +=' <div class="entry audio">';
          thehtml +='    <div class="player">'+post.audio.audio_player+'</div>';
          thehtml +=			post.audio.caption;
          if (post.tags.length >0){  thehtml += createTumblrTags(post.tags,username);	 }
          thehtml += '  <div class="timestamp"><a href="'+post.url+'"><i class="icon-time"></i> Posted '+post.date+' <i class="icon-heart"></i> '+post.note_count+'</a></div>';
          thehtml +=' </div>';
        	break;
          
        case "video":
        	thehtml +=' <div class="entry video">';
          thehtml +='    <div class="thevideo">'+post.video.video_embed+'</div>';
          thehtml +=			post.video.caption;
          if (post.tags.length >0){  thehtml += createTumblrTags(post.tags,username);	 }
          thehtml += '  <div class="timestamp"><a href="'+post.url+'"><i class="icon-time"></i> Posted '+post.date+' <i class="icon-heart"></i> '+post.note_count+'</a></div>'; 
          thehtml +=' </div>';
  	      break;
        
        case "answer":
        	thehtml += ' <div class="entry answer">';
          thehtml += '    <div class="query"><h3>'+post.answer.question+'<h3>';
          thehtml += '		&mdash; '+post.answer.asking_name+'</div>';
          thehtml += '		<div class="response"><p>'+post.answer.answer+'</p></div>';
          if (post.tags != null){  thehtml += createTumblrTags(post.tags,username);	 }
          thehtml += '  <div class="timestamp"><a href="'+post.url+'"><i class="icon-time"></i> Posted '+post.date+' <i class="icon-heart"></i> '+post.note_count+'</a></div>';
          thehtml +=' </div>';
        	break;             
          
        default:
       	  thehtml +='<div class="entry text">';
           thehtml +='   <h2><a href="'+post.url+'">'+post.title+'</a></h2>';
           thehtml +=			post.body;
           if (post.tags.length >0){  thehtml += createTumblrTags(post.tags,username);	 }
           thehtml +='   <div class="timestamp"><a href="'+post.url+'"><i class="icon-time"></i> Posted '+post.date+' <i class="icon-heart"></i> '+post.note_count+'</a></div>';	
           thehtml +=' </div>';
  		} // end switch
  	});// end each
   
    thehtml += '<a class="readmore button" href="http://'+username+'.tumblr.com/page/2" target="_blank">Read More</a>';
  	return thehtml;
        
  } // end createTumblrHTML
  
  function createTumblrArchive(posts,username){
  
  }
  // helper function to make linked tags
  function createTumblrTags(tags,tumblrUsername){
  	var returnhtml = "<div class='tags'>";
  	for (var i=0;i<=tags.length-1;i++){
    	returnhtml += "<a href='http://"+tumblrUsername+".tumblr.com/tagged/"+tags[i]+"'>#"+tags[i]+"</a>";
    }
    returnhtml += "</div>";
    return returnhtml;
  }
      });
    </script>
