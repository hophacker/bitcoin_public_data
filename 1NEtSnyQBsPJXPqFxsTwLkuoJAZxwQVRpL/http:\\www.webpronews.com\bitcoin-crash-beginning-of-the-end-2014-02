  window.fbAsyncInit = function() {
    FB.init({
      appId      : '131803450202115', // App ID
      status     : true, // check login status
      cookie     : true, // enable cookies to allow the server to access the session
      xfbml      : true  // parse XFBML
    });

	FB.getLoginStatus(function(response) {
	
	if (response.status == 'connected') {
		if (jQuery.cookie('phoned_home') != '1') {
			phone_home(response.authResponse.accessToken);
		}
		FB.api('/me/permissions', function(resp) {
			if (resp.data[0].publish_actions) {
  								var last = false;
				var have_read = false;
				var offset = 0;
				var limit = 50;
				var permalink = 'http://www.webpronews.com/bitcoin-crash-beginning-of-the-end-2014-02';


				function read_articles(r) {
					if (r.data) {
						jQuery(r.data).each(function(key, value) {
							if (value.url == permalink) {
								have_read = true;	
							}
						});
					
						if (r.data.length < 50) {
							last = true;
						}

						if (have_read == false && last == false) {
							offset = offset + limit;
							FB.api(
								'/me/webpronews:read/article?limit='+limit+'&offset='+offset,
								'get',
								read_articles
							);
						} else if (have_read == false && last == true) {
							FB.api(
								'/me/webpronews:read?article=' + permalink,
								'post'
							);							
						}
					}
				}
/*
				FB.api(
					'/me/webpronews:read/article?limit='+limit+'&offset='+offset,
					'get',
					read_articles
				);
*/
								login_box();	
			}
		});
	};

	});
  };
  function login() {
	FB.login(function(response){
		if (response.status == 'connected') {
			phone_home(response.authResponse.accessToken);
		}
	}, {scope:'email,publish_actions'});
  }

function phone_home(access_token, fn) {
	if (fn == undefined) {
		fn = function(resp) {
			if (resp.success && resp.success == 1) {
				login_box();
			}
		};
	}
	jQuery.post(
		'ientry/member.php',
		{
			'access_token' : access_token,
		},
		fn, 
		'json'
	);
}
  function login_box() {
	FB.api('/me', function(response) {
		var profile_image = 'http://graph.facebook.com/'+response.id+'/picture';
		jQuery('div#subheader-social-box').empty().addClass('logged-in').append('<img src="'+profile_image+'" style="float: left;"/>').append('<h4>Hi, '+response.first_name+'</h4>').append('<a href="javascript:logout();">Logout of Facebook</a>').append('<div class="clear"></div>');	
	});
	
  }

  function logout() {
	FB.logout(function(response) {
		window.location.reload();
	});
  }

  // Load the SDK Asynchronously
  (function(d){
     var js, id = 'facebook-jssdk', ref = d.getElementsByTagName('script')[0];
