  <div class="share-button" id="share-button-tumbler"> <a href="http://www.tumblr.com/share" title="Share on Tumblr" style="display:inline-block; text-indent:-9999px; overflow:hidden; width:61px; height:20px; background:url('http://platform.tumblr.com/v1/share_2.png') top left no-repeat transparent;" onclick="_gaq.push(['_trackEvent', 'BatteredJoystick Tumbler', 'Share', 'From URL: http://www.batteredjoystick.com/2013/10/21/the-iconic-mario-kart/']);">Share on Tumblr</a> </div>

  <div class="share-button" id="share-button-reddit" style="float: left; margin-left: 5px; background-color: white; padding: 1px 10px 0 10px; border-radius: 3px;"> 
    <script type="text/javascript" src="http://www.reddit.com/buttonlite.js?i=5&newwindow=1"></script> 
  </div>
  <div class="share-button" id="share-button-stumbleUpon">
