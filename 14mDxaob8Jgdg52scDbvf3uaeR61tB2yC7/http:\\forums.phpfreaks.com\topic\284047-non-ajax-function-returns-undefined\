  &#036;dbSalt = &#39;&#036;2a&#036;07&#036;&#39;.substr(hash(&#39;whirlpool&#39;,&#036;word),0,22);
  &#036;dbPass = crypt(&#036;word, &#036;dbSalt);
 return substr(&#036;dbPass,12);
