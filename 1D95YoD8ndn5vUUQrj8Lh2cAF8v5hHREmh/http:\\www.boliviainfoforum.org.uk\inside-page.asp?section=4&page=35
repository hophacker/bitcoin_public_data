<script language="javascript">
function toggle(element_to_toggle, on_off)
{
	if (document.getElementById(element_to_toggle))
	{
		if (on_off == 'on')
		{
			document.getElementById(element_to_toggle).style.display='block';
		}
		else
		{
			document.getElementById(element_to_toggle).style.display='none';
		}
	}
}
</script>

