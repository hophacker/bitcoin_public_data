(1 &lt;&lt; WGM12);&nbsp; &nbsp;// Set CS10 and CS12 bits for 1024 prescaler:<br />&nbsp; TCCR1B |= (1 &lt;&lt; CS10);&nbsp; &nbsp; // prescaler 1024<br />&nbsp; TCCR1B |= (1 &lt;&lt; CS12);&nbsp; &nbsp; // prescaler 1024<br />&nbsp; TIMSK1 |= (1 &lt;&lt; OCIE1A);&nbsp; // enable timer compare interrupt:<br />&nbsp; TIMSK1 |= (1 &lt;&lt; OCIE1B);&nbsp; // enable timer compare interrupt:<br />&nbsp; sei();&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; // Enable global interrupts<br />}<br /><br />void loop()<br />{}<br />ISR(TIMER1_COMPA_vect){digitalWrite(LEDPIN, HIGH);}<br />ISR(TIMER1_COMPB_vect){digitalWrite(LEDPIN, LOW);}<br /></div></div>
