var cookieAppearanceDelay = 5000;
var cookieMessageLifeSpan = 20000;
function hideCookieBox(){
$('#cookieBar').fadeOut();
}
function showCookieBox(){
$('#cookieBar').show();
$('#cookieBar').fadeIn();
setTimeout(function(){hideCookieBox()},cookieMessageLifeSpan);
}
