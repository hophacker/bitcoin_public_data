     <script type="text/javascript">
         $(function () {
             $("#navbar ").stick_in_parent()
             $(".accountcreate").click(function (e) {
                 $('#myModal').modal('show')
             });

             $('#myModal').on('hidden.bs.modal', function (e) {
                 location.reload();
             })
             

             $(".accountlogin").click(function (e) {
                 $('#myModal2').modal('show')
             });

             $("body").on("click", ".create", function (e) {
                 $.post("/account/create", $(".createform").serialize(), function (data) {
                     $('#myModal').html(data.view);
                 });
             });

             $("body").on("click", ".login", function (e) {
                 $.post("/account/login", $(".loginform").serialize(), function (data) {
                     if (data.succes) {
                         location.reload();
                     }
                     $('#myModal2').html(data.view);
                 });
             });



         });
