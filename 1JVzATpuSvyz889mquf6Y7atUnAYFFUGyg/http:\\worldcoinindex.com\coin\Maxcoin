    <script type="text/javascript">

        function DropDown(el) {
            this.dd = el;
            this.placeholder = this.dd.children('span');
            this.opts = this.dd.find('ul.dropdown > li');
            this.val = '';
            this.index = -1;
            this.initEvents();
        }
        $(function () {
            
            var koers = "USD";
            var coinnaam = "Maxcoin";
            var koersdata = "";


            DropDown.prototype = {
                initEvents: function () {
                    var obj = this;
                    obj.dd.on('click touchstart', function (event) {
                        $(this).toggleClass('active');
                        return false;
                    });
                    obj.dd.on('mouseover', function (event) {
                        $(this).toggleClass('active');
                        return false;
                    });
                    obj.dd.on('mouseout', function (event) {
                        $(this).removeClass('active');
                        return false;
                    });
                    obj.opts.on('click touchstart', function () {
                        $('.wrapper-dropdown-5 li').each(function () {
                            $(this).show();
                        });
                        var opt = $(this);
                        opt.hide();
                        obj.val = opt.text();
                        obj.index = opt.index();
                        obj.placeholder.text(obj.val);
                        koers = obj.val
                        $.post("/home/SetCurrencyCookie", { currency: koers }, function (e) {
                            location.reload();
                        });
                 
                    });
                },
                getValue: function () {
                    return this.val;
                },
                getIndex: function () {
                    return this.index;
                }
            }

            var dd = new DropDown($('#dd'));
            $(document).click(function () {
                // all dropdowns
                $('.wrapper-dropdown-5').removeClass('active');
            });


            function generate(data, setkoers) {
                var d = [];
                if (setkoers == "BTC") {
                    $.each(data, function (i, item) { if (item.KoerPrice.BTC != 0) d.push([item.Time.replace("/Date(", "").replace(")/", ""), item.KoerPrice.BTC]) });
                }
                else if (setkoers == "USD") {
                    $.each(data, function (i, item) { if (item.KoerPrice.USD != 0) d.push([item.Time.replace("/Date(", "").replace(")/", ""), item.KoerPrice.USD]) });
                }
                else if (setkoers == "CNY") {
                    $.each(data, function (i, item) { if (item.KoerPrice.CNY != 0) d.push([item.Time.replace("/Date(", "").replace(")/", ""), item.KoerPrice.CNY]) });
                }
                else if (setkoers == "EUR") {
                    $.each(data, function (i, item) { if (item.KoerPrice.EUR != 0) d.push([item.Time.replace("/Date(", "").replace(")/", ""), item.KoerPrice.EUR]) });
                }
                else if (setkoers == "GBP") {
                    $.each(data, function (i, item) { if (item.KoerPrice.GBP != 0) d.push([item.Time.replace("/Date(", "").replace(")/", ""), item.KoerPrice.GBP]) });
                }
                else if (setkoers == "RUR") {
                    $.each(data, function (i, item) {if(item.KoerPrice.RUR != 0) d.push([item.Time.replace("/Date(", "").replace(")/", ""), item.KoerPrice.RUR]) });
                }
                return d;
            }

            GetData(7);
            GetData2(7);
            $('.tradetable').on("click touchstart", ".graphdays", function () {
                $(".graphdays").removeClass("active");
                $('.download-graph').attr('disabled', 'disabled')
                $(this).addClass("active")
               
             
                GetData($(this).data("days"));
              
            });

            $('.tradetable').on("click touchstart", ".graphdays2", function () {
                $(".graphdays2").removeClass("active");
                $('.download-graph2').attr('disabled', 'disabled')
                $(this).addClass("active")

                GetData2($(this).data("days"));
              

            });

            function GetData(days) {
                $('#placeholder').html('');
                $.ajax({
                    type: "POST",
                    url: "/Home/GetGraphdata",
                    dataType: "json",
                    data: { marketid: coinnaam, days: days },
                    success: function (data) {
                        koersdata = data;
                        createGraph(koersdata);
                    },
                    error: function () { }
                });
            }


            function GetData2(days) {
                $('#placeholder2').html('');
                $.ajax({
                    type: "POST",
                    url: "/Home/GetGraphdataVolume",
                    dataType: "json",
                    data: { marketid: coinnaam, days: days },
                    success: function (data) {
                        koersdata = data;
                        createGraph2(koersdata);
                    },
                    error: function () { }
                });
            }

            function createGraph(data) {
                $('#placeholder').html('');
               
                var data1;
                var startdata = [];
                if (koers == "USD") {
                    startdata.push([data.Start.Time.replace("/Date(", "").replace(")/", ""), data.Start.KoerPrice.USD])
                    data1 = [{ label: data.Data[0].Primarycode+"/USD", data: generate(data.Data, "USD") }, { data: startdata }];
                }
                else if (koers == "CNY") {
                    startdata.push([data.Start.Time.replace("/Date(", "").replace(")/", ""), data.Start.KoerPrice.CNY])
                    data1 = [{ label: data.Data[0].Primarycode + "/CNY", data: generate(data.Data, "CNY") }, { data: startdata }];
                }
                else if (koers == "BTC") {
                    startdata.push([data.Start.Time.replace("/Date(", "").replace(")/", ""), data.Start.KoerPrice.BTC])
                    data1 = [{ label: data.Data[0].Primarycode + "/BTC", data: generate(data.Data, "BTC") }, { data: startdata }];
                }

                else if (koers == "EUR") {
                    startdata.push([data.Start.Time.replace("/Date(", "").replace(")/", ""), data.Start.KoerPrice.EUR])
                    data1 = [{ label: data.Data[0].Primarycode + "/EUR", data: generate(data.Data, "EUR") }, { data: startdata }];
                }

                else if (koers == "GBP") {
                    startdata.push([data.Start.Time.replace("/Date(", "").replace(")/", ""), data.Start.KoerPrice.GBP])
                    data1 = [{ label: data.Data[0].Primarycode + "/GBP", data: generate(data.Data, "GBP") }, { data: startdata }];
                }

                else if (koers == "RUR") {
                    startdata.push([data.Start.Time.replace("/Date(", "").replace(")/", ""), data.Start.KoerPrice.RUR])
                    data1 = [{ label: data.Data[0].Primarycode + "/RUR", data: generate(data.Data, "RUR") }, { data: startdata }];
                }

                var plot = $.plot("#placeholder", data1, {
                    series: {
                        lines: {
                            show: true,
                            lineWidth: 1
                        },
                    },
                    // colors: ["#92d5ea"],
                    grid: {
                        hoverable: false,
                        width: 818,
                        height: 413
                    },
                    xaxis: {
                        mode: "time",
                        timezone: "America/Chicago",
                        labelWidth: 1,
                    },
                });
                html2canvas($('.container1'), {
                    onrendered: function (canvas) {
                        $('#imageData').val(canvas.toDataURL().replace('data:image/png;base64,', ''));
                        $('.download-graph').attr('disabled', false);
                    }
                });
            }


            function createGraph2(data) {
                $('#placeholder2').html('');

                var data1;
                var startdata = [];
                if (koers == "USD") {
                    startdata.push([data.Start.Time.replace("/Date(", "").replace(")/", ""), data.Start.KoerPrice.USD])
                    data1 = [{ label: data.Data[0].Primarycode + "/USD", data: generate(data.Data, "USD") }, { data: startdata }];
                }
                else if (koers == "CNY") {
                    startdata.push([data.Start.Time.replace("/Date(", "").replace(")/", ""), data.Start.KoerPrice.CNY])
                    data1 = [{ label: data.Data[0].Primarycode + "/CNY", data: generate(data.Data, "CNY") }, { data: startdata }];
                }
                else if (koers == "BTC") {
                    startdata.push([data.Start.Time.replace("/Date(", "").replace(")/", ""), data.Start.KoerPrice.BTC])
                    data1 = [{ label: data.Data[0].Primarycode + "/BTC", data: generate(data.Data, "BTC") }, { data: startdata }];
                }

                else if (koers == "EUR") {
                    startdata.push([data.Start.Time.replace("/Date(", "").replace(")/", ""), data.Start.KoerPrice.EUR])
                    data1 = [{ label: data.Data[0].Primarycode + "/EUR", data: generate(data.Data, "EUR") }, { data: startdata }];
                }

                else if (koers == "GBP") {
                    startdata.push([data.Start.Time.replace("/Date(", "").replace(")/", ""), data.Start.KoerPrice.GBP])
                    data1 = [{ label: data.Data[0].Primarycode + "/GBP", data: generate(data.Data, "GBP") }, { data: startdata }];
                }

                else if (koers == "RUR") {
                    startdata.push([data.Start.Time.replace("/Date(", "").replace(")/", ""), data.Start.KoerPrice.RUR])
                    data1 = [{ label: data.Data[0].Primarycode + "/RUR", data: generate(data.Data, "RUR") }, { data: startdata }];
                }

                var plot = $.plot("#placeholder2", data1, {
                    series: {
                        lines: {
                            show: true,
                            lineWidth: 1
                        },
                    },
                    // colors: ["#92d5ea"],
                    grid: {
                        hoverable: false,
                        width: 818,
                        height: 413
                    },
                    xaxis: {
                        mode: "time",
                        timezone: "America/Chicago",
                        labelWidth: 1,
                    },
                });
                html2canvas($('.container2'), {
                    onrendered: function (canvas) {
                        $('#imageData2').val(canvas.toDataURL().replace('data:image/png;base64,', ''));
                        $('.download-graph2').attr('disabled', false);
                    }
                });
            }

        });


    </script>




   



