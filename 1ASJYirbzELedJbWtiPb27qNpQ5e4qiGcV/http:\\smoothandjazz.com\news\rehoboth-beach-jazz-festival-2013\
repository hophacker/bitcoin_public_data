	<script>
		var socket = io.connect('http://sf.amazingradios.com:4444');
		socket.on('connect', function() { 
			socket.emit('request_room',"arjazzhigh");
			console.log('connected');
		});
		socket.on('song_change', function (data) {
			console.log(data);
	                jQuery('#current_song').text(data.now);
			jQuery('#last5').html('');
			data['last'].forEach(function(entry) {
				jQuery('#last5').append('<li>'+entry+'</li>')
			});
		});
	</script>
