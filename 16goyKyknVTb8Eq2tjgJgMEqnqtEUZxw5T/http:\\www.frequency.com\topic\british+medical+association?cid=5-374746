"},{"label":"science", "url":"/topic/science?cid=5-439"},{"label":"space", "url":"/topic/space?cid=5-479"},{"label":"taxes", "url":"/topic/taxes?cid=5-14431"},{"label":"temperance movement", "url":"/topic/temperance+movement?cid=5-847010"},{"label":"the decision", "url":"/topic/the+decision?cid=5-265956"},{"label":"the pattern", "url":"/topic/the+pattern?cid=5-7872293"},{"label":"the rise", "url":"/topic/the+rise?cid=5-343500"},{"label":"tobacco", "url":"/topic/tobacco?cid=5-1530"},{"label":"uk government", "url":"/topic/uk+government?cid=5-26713"},{"label":"united kingdom", "url":"/topic/united+kingdom?cid=5-658"},{"label":"velvet glove", "url":"/topic/velvet+glove?cid=5-17098637"},{"label":"women", "url":"/topic/women?cid=5-100"}],
