<script type="text/javascript">
	var cell = $(".tborder[id^='post'] tr:nth-child(3) .alt2"); // Find all post tables
	cell.addClass('custom-tooltip'); // Add custom class

	$('.custom-tooltip').hover(
		function() {
			var parent = $(this);
			$('#custom-tooltip').css('left', parent.position().left);
			$('#custom-tooltip').css('top', parent.position().top + 25);
			$('#custom-tooltip').show();
		},
		function() {
			$('#custom-tooltip').hide();
		}
	);
	
	$('#custom-tooltip').hover(
		function() { $(this).show(); },
		function() { $(this).hide(); }
	);	
</script>
