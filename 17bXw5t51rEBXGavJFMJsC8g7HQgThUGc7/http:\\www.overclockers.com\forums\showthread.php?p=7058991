<script type="text/javascript">
	$(document).ready(function() {
		//Show the registration overlay when en element with the class .guest-reg is clicked
		$(".guest-reg").click(function() {
			$(".overlay-container").show();
		});
		//Hide the overlay when 
		$(".overlay-shadow, .overlay-close").click(function() {
			$(".overlay-container").hide();
		});
	});
</script>
