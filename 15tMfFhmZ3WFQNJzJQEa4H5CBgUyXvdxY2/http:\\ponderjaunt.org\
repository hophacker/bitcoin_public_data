<script>

  /**
   * A temporary way to get cat, tag and author links to work
   * as there's no access to collection.fullUrl from this
   * partial's scope.
   *
   * @param   url JSON-T-derived permalink url to parse
   */
  function createSummaryBlockCollectionUrl(url) {
    var a = document.createElement('a');
    a.href = url;
    window.location = a.origin + '/' + a.pathname.split('/')[1] + a.search;
  };

</script></div>
