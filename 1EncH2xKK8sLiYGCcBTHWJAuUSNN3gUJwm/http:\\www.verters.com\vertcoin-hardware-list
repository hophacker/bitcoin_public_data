<script>
$(document).ready(function() {
	$("span.glyphicon-arrow-up").on("click",function() {
		var th = this;
		$.post("/vertcoin-hardware-list",{ "fn": "arrowUp", "key": $(this).attr("data-key") },function() {
			$("span.glyphicon-arrow-down",$(th).parent()).remove();
			$(th).remove();
		});
	});
	$("span.glyphicon-arrow-down").on("click",function() {
		var th = this;
		$.post("/vertcoin-hardware-list",{ "fn": "arrowDown", "key": $(this).attr("data-key") },function() {
			$("span.glyphicon-arrow-up",$(th).parent()).remove();
			$(th).remove();
		});
	});
});
</script>
