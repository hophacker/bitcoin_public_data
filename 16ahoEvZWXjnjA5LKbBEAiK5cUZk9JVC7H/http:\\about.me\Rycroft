(function(requirejs) {
    /* Configure Raven to handle require() failures a bit more gracefully. */
    if (window.Raven){
        requirejs.onError = function(err){
            /* This captures errors that happen trying to load a module. This
             * can be a "timeout", a "scripterror" (i.e. 404), or a "nodefine".
             * This does NOT include thrown errors and syntax errors.
             * http://requirejs.org/docs/api.html#errbacks
             */
            /* We delete err.originalError because it contains a circular
             * reference that can't be serialized by Raven. We might want to
             * pick out some individual values to include in the extra data we
             * send. */
            delete err.originalError;
            Raven.captureMessage("Require failed", {extra: err});
        };
    }
