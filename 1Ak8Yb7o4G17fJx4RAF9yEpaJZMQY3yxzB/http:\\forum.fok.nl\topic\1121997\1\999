    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();

		
	if($("#mollie-container").length>0) {
		$(window).load(function() {
			var script = document.createElement( 'script' );
			script.type = 'text/javascript';
			script.src = 'https://www.mollie.nl/partners/microads/1125297.js';
			$("body").append( script );
		});
	}
	

var noAdsense=0;


	var megatag_id = 0;
	if(noAdsense!=1){
		$(".megatag_leaderboard").each(function(index){
			megatag_id++;
			$(this).attr('id', 'megatag_'+megatag_id);
			adnxs.megatag.load({
				targetId: $(this).attr('id'),
				size: $(this).attr('data-size'),
				promoSizes : ['1x1', $(this).attr('data-size')]
			});
		});
		
		$(".megatag_leaderboard_big").each(function(index){
			megatag_id++;
			$(this).attr('id', 'megatag_'+megatag_id);
			adnxs.megatag.load({
				targetId: $(this).attr('id'),
				size: '728x90',
				promoSizes : ['1x1', $(this).attr('data-size')]
			});
		});
		
	}

