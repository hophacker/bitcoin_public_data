function shareThisIconHack(shareId){
var shareInnerHtml = $(shareId).html();
if(shareInnerHtml==='New'){
$(shareId).html('0');
}
}
$(document).ready(function(){
var shareIdsObj={
id1: '#sharecountTop',
id2: '#sharecountBottom'
};
for(var shareId in shareIdsObj){
shareThisIconHack(shareIdsObj[shareId]);
}
});
</script>
