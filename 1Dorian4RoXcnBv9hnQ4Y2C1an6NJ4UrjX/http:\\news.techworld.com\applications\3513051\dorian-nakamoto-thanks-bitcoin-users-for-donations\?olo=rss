<script>
var gptEscape = function(s){
s = s.replace(/[^a-zA-Z0-9_\- ]/gim,'');
s = s.replace(/[ ]+/gi,'+');//replace multi space with +
return s;
};
var adConfig = {
debug : false,
zone : 'applications'.toLowerCase(),
units : AD_CONFIG_UNITS
};
if(adConfig.debug)console.log(adConfig);
googletag = window.googletag || {cmd:[]};
var adsToRefresh = [];
function refreshAds() {
if(adConfig.debug)console.log('refreshAds() called');
googletag.pubads().refresh(adsToRefresh);
}
googletag.cmd.push(function() {
var mappinglbpos1 = googletag.sizeMapping().
addSize([980, 0], [[970, 250], [728, 90]]).
addSize([570, 0], [[728, 90]]).
addSize([478, 0], [[468, 60]]).
addSize([280, 0], [[320, 50], [300, 50]]).
addSize([0, 0], [[12, 1]]).
build();
var mappingmpupos1 = googletag.sizeMapping().
addSize([830, 0], [[300, 250], [300, 600]]).
addSize([0, 0], [12, 1]).
build();
var mappingwelcomead = googletag.sizeMapping().
addSize([650, 0], [1, 1]).
build();
var mappingSkyAd = googletag.sizeMapping().
addSize([0,0], [[120,600],[160,600],[160,800]]).
build();
/*
var mappingButtonAd = googletag.sizeMapping().
addSize([0,0], [[160,160]]).
build();
*/
var mappingInskin = googletag.sizeMapping().
addSize([0,0], [[2,2]]).
build();
googletag.pubads().collapseEmptyDivs();
googletag.pubads().enableAsyncRendering();
googletag.pubads().enableVideoAds();
var pubAds = [],
mpuPos = 1,
lbPos = 1,
skyPos = 1,
buttonPos = 1,
inskinPos = 1,
slotPrefix = '/6350/techworld.com/';
if ("welcomead-pos1" in adConfig.units) {
if(adConfig.debug)console.log('enabling welcomead');
pubAds.push(googletag.defineOutOfPageSlot(slotPrefix + adConfig.zone, 'welcomead-pos1').defineSizeMapping(mappingwelcomead).addService(googletag.pubads()));
}
for (var unit in adConfig.units) {
var pos = -1;
if(unit.indexOf("leaderboard")!==-1){
var ad = googletag.defineSlot(slotPrefix + adConfig.zone, [728, 90], 'leaderboard-pos'+lbPos).defineSizeMapping(mappinglbpos1).addService(googletag.pubads()).setTargeting("pos", [''+lbPos]);
pos = lbPos+=1;
}else if(unit.indexOf("mpu")!==-1){
var ad = googletag.defineSlot(slotPrefix + adConfig.zone, [300, 250], 'mpu-pos'+mpuPos).defineSizeMapping(mappingmpupos1).addService(googletag.pubads()).setTargeting("pos", [''+mpuPos]);
pos = mpuPos+=1;
}else if(unit.indexOf("sky-")!==-1){
var ad = googletag.defineSlot(slotPrefix + adConfig.zone, [120, 600], 'sky-pos'+skyPos).defineSizeMapping(mappingSkyAd).addService(googletag.pubads()).setTargeting("pos", [''+skyPos]);
pos = skyPos+=1;
//}else if(unit.indexOf("button-")!==-1){
//var ad = googletag.defineSlot(slotPrefix + adConfig.zone, [160, 160], 'button-pos'+buttonPos).defineSizeMapping(mappingButtonAd).addService(googletag.pubads()).setTargeting("pos", [''+buttonPos]);
//pos = buttonPos+=1;
}else if(unit.indexOf("inskin-")!==-1){
var ad = googletag.defineSlot(slotPrefix + adConfig.zone, [2, 2], 'inskin-pos'+inskinPos).defineSizeMapping(mappingInskin).addService(googletag.pubads()).setTargeting("pos", [''+inskinPos]);
pos = inskinPos+=1;
}else{
if(adConfig.debug)console.log('skipping '+unit);
continue;
}
if(adConfig.debug)console.log('enabling '+unit+',pos='+(pos-1));
pubAds.push(ad);
adsToRefresh.push(ad);
}
var resizeTimeOut = false;
window.onresize = function(){
if(resizeTimeOut !== false){
clearTimeout(resizeTimeOut);
}
resizeTimeOut = setTimeout(refreshAds, 500);
};
//meta tags
var rs_tax = [];
var metatags = jQuery('meta[name=keywords]').attr("content");
if (metatags != undefined && metatags != '') {
strmetatags = metatags.toLowerCase().replace(/&amp;/g, "").replace(/&/g, "").split(",");
for(var i=0, len=strmetatags.length; value=strmetatags[i], i<len; i++) {
rs_tax.push(gptEscape(jQuery.trim(value)));
}
}
if (rs_tax.length > 0) {
if(adConfig.debug)console.log('kw=' + rs_tax.join(','));
googletag.pubads().setTargeting("kw", rs_tax);
}
// add krux ids
if(adConfig.debug)console.log('ksg='+Krux.segments);
if(adConfig.debug)console.log('kuid='+Krux.user);
if(adConfig.debug)console.log('khost='+location.hostname);
googletag.pubads().setTargeting("ksg", Krux.segments);
googletag.pubads().setTargeting("kuid", Krux.user);
googletag.pubads().setTargeting("khost", encodeURIComponent(location.hostname));
googletag.pubads().setTargeting("URL", encodeURIComponent(window.location));
googletag.pubads().setTargeting("artid", '3513051');
googletag.pubads().setTargeting('chan', gptEscape('applications'));
if(adConfig.debug)console.log('chan='+gptEscape('applications'));
googletag.pubads().setTargeting('sec', gptEscape('applications'));
if(adConfig.debug)console.log('sec='+gptEscape('applications'));
//bring it on!
googletag.pubads().enableSingleRequest();
googletag.enableServices();
});
//setTimeout(function(){googletag.pubads().refresh();},6000);//test for ad refresh
</script>
