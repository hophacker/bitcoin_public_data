						},
						'mouseleave': function() {
							fx.start(featOpacity);
							if (fxColors.length) { fxColors.each(function(fxC) { fxC.start(color); }); }
							if (fxH3s.length) {	fxH3s.each(function(fxH) { fxH.start(color); }); }
						}
					})
				});
