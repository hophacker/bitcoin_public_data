	  	  <!-- Item title -->
	  <h2 class="itemTitle">
						
	  	My holy book
	  	
	  </h2>
	  
				<!-- Item Author -->
		<span class="itemAuthor">
			Written by&nbsp;
						<a rel="author" href="/index.php/home/itemlist/user/65-nayaaerodiode">Naya Aerodiode</a>
					</span>
		
  </div>

  <!-- Plugins: AfterDisplayTitle -->
  
