<article id="post-1888" class="post-1888 post type-post status-publish format-standard hentry category-sensitive tag-feel tag-good tag-help tag-individual tag-intentions tag-kind tag-kindness tag-need">
	
	<header class="entry-header">
					<span class="entry-meta entry-meta-categories"><a href="http://peppoj.net/category/sensitive/" title="View all posts in Sensitive" rel="category tag">Sensitive</a></span>
				<h1 class="entry-title">Kill with Kindness</h1>		<p class="entry-meta entry-meta-time"><i class="fa fa-clock-o"></i>July 18, 2011</p>
	</header><!-- .entry-header -->

	<div class="entry-content">
		
<!-- Easy AdSense V7.22 -->
<!-- [leadin: 0 urCount: 0 urMax: 0] -->
<div class="ezAdsense adsense adsense-leadin" style="text-align:center;margin:12px;"><script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
<!-- Post ad -->
<ins class="adsbygoogle"
     style="display:inline-block;width:468px;height:60px"
     data-ad-client="ca-pub-3571877886198893"
     data-ad-slot="2964431411"></ins>
