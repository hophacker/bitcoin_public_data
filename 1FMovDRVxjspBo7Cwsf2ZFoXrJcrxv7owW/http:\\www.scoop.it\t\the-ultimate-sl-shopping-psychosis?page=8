<script type="text/javascript" >

	function getStatsForThemeMiniStats() {
		// Call ajax request only if data is not here.
		if ($("#upv_426314").text() == "Loading..." ) {
			jQuery.getJSON("/stats/theme/426314?view=json", function(json){
				jQuery("#upv_426314").empty();
				if (json.stats && json.stats.uniqueVisitors >= 0) {
					jQuery("#upv_426314").append(json.stats.uniqueVisitors);				
				} else {
					jQuery("#upv_426314").append("Not available");
				}
			});
		}
	}
</script>
