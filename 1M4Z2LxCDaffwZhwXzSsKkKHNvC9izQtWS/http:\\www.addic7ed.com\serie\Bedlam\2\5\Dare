	function showChange(showID)
	{
		if (showID==0)
			valor = $("qsShow").value;
		else
			valor = showID;
		if (valor>0)
		{
			$("qsSeason").innerHTML = '<img title="Please wait, loading" alt="Loading" src="http://cdn.addic7ed.com/images/loader.gif" />';
			$("qsEp").innerHTML = '';
			$("qssShow").innerHTML = '';

