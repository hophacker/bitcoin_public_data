		            }
                        });
                        myRequest.send('showID='+valor+'&season='+myseason); 
		}

	}
	
	function changeEp()
	{
		var valor = $("qsiEp").value;
		window.location = '/re_episode.php?ep=' + valor;
	}
	
	function qsClear()
	{
		$("qssShow").innerHTML = '<img title="Please wait, loading" alt="Loading" src="http://cdn.addic7ed.com/images/loader.gif" />';
