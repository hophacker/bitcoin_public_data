			    url: '/ajax_getSeasons.php',
			    method: 'get',
			    onSuccess: function(responseText){
				$("qsSeason").innerHTML = responseText;
			    }
			});
			myRequest.send('showID='+valor);
		}
	}
	
	function seasonChange(showID, season)
	{
	
		valor = showID;

		if (valor>0)
