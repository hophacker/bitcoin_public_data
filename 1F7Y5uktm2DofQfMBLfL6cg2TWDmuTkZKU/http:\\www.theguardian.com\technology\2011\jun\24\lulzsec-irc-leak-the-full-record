            <script type="text/javascript">
        // thank you http://www.electrictoolbox.com/pad-number-zeroes-javascript/
        function pad(number, length) {
            var str = '' + number;
            while (str.length < length) {
                str = '0' + str;
            }
            return str;
        };
        var now = new Date();
        
        s.eVar1 = now.getFullYear() + '/' +
            pad(now.getMonth() + 1, 2) + '/' +
            pad(now.getDate(), 2);
    </script>

