<script type="text/javascript" >

	function getStatsForThemeMiniStats() {
		// Call ajax request only if data is not here.
		if ($("#upv_587345").text() == "Loading..." ) {
			jQuery.getJSON("/stats/theme/587345?view=json", function(json){
				jQuery("#upv_587345").empty();
				if (json.stats && json.stats.uniqueVisitors >= 0) {
					jQuery("#upv_587345").append(json.stats.uniqueVisitors);				
				} else {
					jQuery("#upv_587345").append("Not available");
				}
			});
		}
	}
</script>
