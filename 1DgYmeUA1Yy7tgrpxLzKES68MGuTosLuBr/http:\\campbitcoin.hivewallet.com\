					<div id="mc_embed_signup">
					<form action="http://campbitcoin.us3.list-manage2.com/subscribe/post?u=abab72e22d37427d55e0ce144&amp;id=9c3e1b2bed" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
					<div class="indicates-required"><span class="asterisk"></span> </div>
					<div class="mc-field-group">
						<label for="mce-EMAIL">
					</label>
						<input type="email" value="" placeholder="Email Address" name="EMAIL" class="required email input_signup" id="mce-EMAIL">
					</div>
					<div class="mc-field-group">
						<label for="mce-FNAME"> </label>
						<input type="text" value="" placeholder="Name and Affiliation" name="FNAME" class="input_signup" id="mce-FNAME">
					</div>
					<div class="mc-field-group">
						<textarea value="" placeholder=" What do you hope to contribute to the camp?" name="CONTRIBUTE" class="input_signup" id="mce-CONTRIBUTE"></textarea>
