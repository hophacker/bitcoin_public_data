      var config = {'blogId': '38040511487295963', 'postId': '3570651810390844552', 'feed': 'http://anonymity-in-bitcoin.blogspot.com/feeds/3570651810390844552/comments/default', 'authorName': 'Martin Harrigan', 'authorUrl': 'http://www.blogger.com/profile/01610696350600503266', 'baseUri': 'http://www.blogger.com', 'maxThreadDepth': 2};

