    <script type="text/javascript"> 
    //<![CDATA[
             function init_gn(){
            gn = 'ml';
        }
        function change(objects){
            if (gn == 'ml'){
                gn = 'fm';
            } else {
                gn = 'ml';
            }
        	for (i=0; i < objects.length; i++) {
                if ( objects[i].id.indexOf('-ml') != -1 || objects[i].id.indexOf('-fm') != -1 ){
                	if ( objects[i].style.display != 'none' ) {
                		objects[i].style.display = 'none';
                	}
                	else {
                		objects[i].style.display = 'inline';
                    }
            	}
        	}
        }
        function change_gn(){
            change(document.getElementsByTagName('span'));
            change(document.getElementsByTagName('textarea'));
            change(document.getElementsByTagName('a'));
        }
        //]]>
    </script>
</head>

