<script type="text/javascript">

function notifyBigger( msg )
{
	var body = document.getElementsByTagName("body")[0];
	var first = body.firstChild;
	var noti = first;
	if(first.tagName != "DIV" || first.id != "notification-bigger")
	{
		var h = 64;

		noti = document.createElement("DIV");
		noti.id = "notification-bigger";
		body.insertBefore(noti, first);

		var spacer = document.createElement("DIV");
		spacer.id = "notification-spacer-bigger";
		spacer.style.position="relative";
		spacer.style.display="block";
		spacer.style.left="0px";
		spacer.style.top="0px";
		spacer.style.width="100%";
		spacer.style.height=h+"px";
		body.insertBefore(spacer, first);
	}
	noti.innerHTML = msg;
}

function notify( msg )
{
	var body = document.getElementsByTagName("body")[0];
	var first = body.firstChild;
	var noti = first;
	if(first.tagName != "DIV" || first.id != "notification")
	{
		var h = 21;

		noti = document.createElement("DIV");
		noti.id = "notification";
		body.insertBefore(noti, first);

		var spacer = document.createElement("DIV");
		spacer.id = "notification-spacer";
		spacer.style.position="relative";
		spacer.style.display="block";
		spacer.style.left="0px";
		spacer.style.top="0px";
		spacer.style.width="100%";
		spacer.style.height=h+"px";
		body.insertBefore(spacer, first);
	}
	noti.innerHTML = msg;
}

</script>


