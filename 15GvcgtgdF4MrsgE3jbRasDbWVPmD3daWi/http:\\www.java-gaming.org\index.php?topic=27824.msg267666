		var pageLoadTitle = document.title;
		function updatePostsSincePageLoad()
		{
			var bar_elem = document.getElementById("posts_since");
			var url  = "http://www.java-gaming.org:8080/push-notifications?msgid="+prevNumMsgIds+"&topicid=27824&rndn=" + (new Date().getTime());
			getXMLDocument(url, function( xmlRoot )
				{
					var value = xmlRoot.childNodes[0].attributes["value"].nodeValue;
					var title = xmlRoot.childNodes[0].attributes["title"].nodeValue;

					if(bar_elem != null) {
						bar_elem.innerHTML = title;
					}
					
					if(title != "") {
						document.title = title+" "+pageLoadTitle;
					
						notify(value);
					}
					
					// ... and again!					
					setTimeout(updatePostsSincePageLoad, 1000);
				}
			);
		}
		if(!isMSIE) {
			setTimeout(updatePostsSincePageLoad, 4000);
		}
	</script>
	
		

	

