                var playerVersion = swfobject.getFlashPlayerVersion();
        var flashPlayerVersion = playerVersion.major + "." + playerVersion.minor + "." + playerVersion.release;

        
                                                
                        
                
                
        
                
                
        var utag_data={
            flash_player_version:flashPlayerVersion,
                    store_language:"/us/en",
                    account_id:"null",
            login_status:"Not Logged in",
            event:["Page View", "Lulu Searches"],
            page_name:"/shop/search.ep"
                    , site_currency:"USD"
                                    , search_keyword:"Bible History"
                                                };
    </script>
    <script type="text/javascript" src="//tealium.hs.llnwd.net/o43/utag/lulu/main/prod/utag.js"></script>
    <script type="text/javascript">
        (function ($) {
            if (!window.utag) {
                return;
            }
            var utag_base_data = {
                                store_language:"/us/en",
                                account_id:"null",
                login_status:"Not Logged in",
                page_name:"/shop/search.ep",
                                site_currency:"USD"
                            };

                        $('form.add-to-cart').each(function(index) {
                $(this).submit(function() {
                    utag_base_data.event="Cart Item Additions";
                    utag_base_data.product_id=[$(this).children('input[name$="skuCode"]').val()];
                    utag.view(utag_base_data);
                });
            });

                        $('ul#accountNavigation > li > a.cart').each(function(index) {
                $(this).click(function() {
                    utag_base_data.event="Cart Views";
                    utag.view(utag_base_data);
                });
            });

                        $('div.remove-cart-item-link > a').each(function(index) {
                $(this).click(function() {
                    utag_base_data.event="Cart Item Removals";
                    utag_base_data.product_id=[$(this).parents("div.remove-cart-item-link").children("div#productId").html()];
                    utag.view(utag_base_data);
                });
            });

                        $('button[name$="cartCheckout"]').each(function(index) {
                $(this).click(function() {
                    utag_base_data.event="Cart Checkout";
                    utag.view(utag_base_data);
                });
            });

                        $('div#shippingOptions > div.save-button > button').each(function(index) {
                $(this).click(function() {
                    utag_base_data.event="Cart Ship Method";
                    utag.view(utag_base_data);
                });
            });

                        $('form#shippingForm > div#addressDetails > div.save-button > button').each(function(index) {
                $(this).click(function() {
                    utag_base_data.event="Cart Shipping Address";
                    utag.view(utag_base_data);
                });
            });

                        $('form#billingForm > div#billingDetails > div.save-button > button').each(function(index) {
                $(this).click(function() {
                    utag_base_data.event="Cart Billing Info";
                    utag.view(utag_base_data);
                });
            });

            window.sendProductReviewEventToTealium = function () {
                utag_base_data.event="Product Review";
                                utag.view(utag_base_data);
            }

            window.sendRegistrationEventToTealium = function (fullName, email, accountId) {
                utag_base_data.event = "registration";
                utag_base_data.email = email;
                utag_base_data.name = fullName;
                utag_base_data.account_id = accountId;
                utag_base_data.login_status = "Logged in";
                utag.view(utag_base_data);
            }

        }(jQuery));
    </script>
