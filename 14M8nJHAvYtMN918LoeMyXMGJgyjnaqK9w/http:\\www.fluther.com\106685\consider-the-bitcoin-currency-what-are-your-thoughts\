<script type="text/javascript">
KnowledgeGame = function() {
	
	function Choice(choice_id, option1, option2) {
		this.choice_id = choice_id;
		this.option1 = option1;
		this.option2 = option2;
	};
	
	Choice.prototype.setup = function(next) {
		this.next = next;
		
		//Setup option text
		var option1 = YD.getElementsByClassName('option1','a',this.choice_id)[0];
		option1.innerHTML = this.option1;
		
		var option2 = YD.getElementsByClassName('option2','a',this.choice_id)[0];
		option2.innerHTML = this.option2;
	
		var links = YD.getElementsByClassName('choice', 'a', this.choice_id);
		for (var i in links) {
			YE.on(links[i], 'click', Bind(function (e) {
			YE.stopEvent(e);
			YD.setStyle(this.choice_id, 'display', 'none');
			YD.setStyle(this.next, 'display', '');
			var targ= YE.getTarget(e);
			$("game_join_link").href += escape(targ.innerHTML + ",");
			FlutherPageTrack("/knowledgegame/" + this.choice_id + "/" + targ.innerHTML)
		}, this));
		}
	};
	
	var knowledge_data =  eval('[[[\u0022Chemistry\u0022, \u0022Politics\u0022], [\u0022College\u0022, \u0022Parenting\u0022], [\u0022Grammar\u0022, \u0022TV Shows\u0022]], 7]');
	
	var choice_list = knowledge_data[0];
	var num_matches = knowledge_data[1];
	
	this.choice1 = new Choice("choice1", choice_list[0][0], choice_list[0][1]);
	this.choice2 = new Choice("choice2", choice_list[1][0], choice_list[1][1]);
	this.choice3 = new Choice("choice3", choice_list[2][0], choice_list[2][1]);
	
	this.choice1.setup("choice2");
	this.choice2.setup("choice3");
	this.choice3.setup("join_time");
	
	$('num_matches').innerHTML = num_matches;
	
	$("game_join_link").href += "&answers=" + escape(num_matches + ",");
	
};

EXTENSIONS.push(KnowledgeGame);
</script>



            
			</div>
			<div id="sidebar">
				
