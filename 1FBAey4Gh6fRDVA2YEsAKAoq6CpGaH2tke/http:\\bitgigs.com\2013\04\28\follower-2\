          <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.4.4/jquery.min.js"></script>
          <script type="text/javascript" src="http://bitgigs.com/wp-content/themes/ninetofive/scripts/jquery.autocomplete.pack.js"></script>
          <script type="text/javascript" src="http://bitgigs.com/wp-content/themes/ninetofive/scripts/jquery.select_skin.js"></script>
          <script type="text/javascript" src="http://bitgigs.com/wp-content/themes/ninetofive/scripts/jquery.pngfix.js"></script>
          <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.6/jquery-ui.min.js"></script>
