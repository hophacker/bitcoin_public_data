    <script type="text/javascript"> 
    jQuery.noConflict();
    jQuery(document).ready( function() {
      // load breaking news now
      jQuery.ajax({
        url: "/ajax/breakingNewsBarContent",
        success: function(message) {
          var returnJson = jQuery.parseJSON(message);
          // set data
          jQuery("#breakingNewsItem").html(returnJson.title);
          jQuery("#breakingNewsItem").attr('href', returnJson.url);
          if(returnJson.title != '') {
        	  jQuery("#breakingNews").show();
          }
        }
      }); 
      // close breaking news
      jQuery("#breakingClose").click( function(e) {
        //
        jQuery("#breakingNews").hide();
        jQuery.ajax({
          url: "/ajax/ignoreBreakingNews",
          /*data: { v: returnJson.blogid },*/
          success: function() {}
        });
      });          
    });
    </script>
    		
