      <script type="text/javascript">
      var infoHeight = jQuery("#description_wrapper .content").css('height');
      function toggleInfo()
      {
          if(jQuery("#description_wrapper .close").css('display') == 'none')
          {
              // open
              jQuery("#description_wrapper .content").css('height', '100%');
              //
              jQuery("#description_wrapper .open").hide();
              jQuery("#description_wrapper .close").show();
          }
          else 
          {
              // close
              jQuery("#description_wrapper .content").css('height', infoHeight);
              //
              jQuery("#description_wrapper .close").hide();
              jQuery("#description_wrapper .open").show();
          }
      }
      </script>
      
