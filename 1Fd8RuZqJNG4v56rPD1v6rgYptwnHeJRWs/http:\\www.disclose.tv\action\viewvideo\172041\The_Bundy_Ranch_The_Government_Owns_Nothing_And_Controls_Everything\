<script type="text/javascript">
jQuery(document).ready( function() {

	// toggle overlay elements
	jQuery(".accordion_toggler").live('click', function() {
		  // 
		  var isDisplayed = (jQuery(this).next(".accordion_content").is(':visible'));
		  // hide the other elements
		  jQuery(".accordion_content").slideUp(200);
		  //
		  if(! isDisplayed)
		  {
			    jQuery(this).next(".accordion_content").slideDown(200);
		  }
	});
});
// 
function showShareOverlay( mediaKey )
{
	// clear content first
	jQuery("#shareWindow").html('');
		
	// load div content
	//if( jQuery("#shareWindow").html() == '' )
	{
  	jQuery.ajax({
  		type: "POST",
  		url: "/media/ajax?action=share",
  		data: {
  			  key: mediaKey
  		},
  		success: function(data) {
  			jQuery("#shareWindow").html( data );
  		},
  		error: function() {
  			alert('error');
  		}
    });
	}
	jQuery("#shareOverlay").show();
	jQuery("#shareWindow").show();
}

//
function hideShareOverlay() {
	jQuery("#shareWindow").hide();
	jQuery("#shareOverlay").hide();
}

//
function showFlagContentOverlay(mediaKey)
{
	// clear content first
	jQuery("#shareWindow").html('');
	
  // load div content
  //if( jQuery("#shareWindow").html() == '' )
  {
    jQuery.ajax({
      type: "POST",
      url: "/media/ajax?action=flagContent",
      data: {
          key: mediaKey
      },
      success: function(data) {
        jQuery("#shareWindow").html( data );
      },
      error: function() {
        alert('error');
      }
    });
  }
  jQuery("#shareOverlay").show();
  jQuery("#shareWindow").show();  
}

//
function hideFlagContentOverlay() { 
	  jQuery("#shareWindow").hide();
	  jQuery("#shareOverlay").hide();  
}

// 
jQuery("#flag_content_send").live('click', function() {
	// gather variables
	var flag_reason = jQuery("#flag_reason").val();
	var flag_text = jQuery("#flag_text").val();
	var flag_mediakey = jQuery("#mediakey").html();
	// 
	// alert('flag_reason = ' + flag_reason + '\n' + 'text = ' + flag_text + '\n' + 'mediakey = ' + flag_mediakey);

	// 
  jQuery.ajax({
	  type: "POST",
    url: "/ajax/flagContent",
    data: {
    	  type: 'media', 
    	  mediakey: flag_mediakey,
    	  reason: flag_reason,
    	  text: flag_text
    },
    success: function(data) {
    	  // 
    	  var json = jQuery.parseJSON(data);
    	  // alert('json.result = ' + json.result);
    	  // undisplay overlay
    	  hideFlagContentOverlay();
    	  // show message
    	  alert('Thank you! We have received your request.');
    },
    error: function() {
    	  alert('error');
    	  }
    });	
});
</script><!-- header -->
