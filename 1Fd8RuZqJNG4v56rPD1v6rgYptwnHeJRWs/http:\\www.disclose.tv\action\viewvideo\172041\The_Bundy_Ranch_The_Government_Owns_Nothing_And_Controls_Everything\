<script type="text/javascript">
var newEmbedCode;
jQuery(document).ready( function() {
	jQuery("#switchComments a").click( function(e) {
		  e.preventDefault();
		  // li id
		  var id = jQuery(this).parent().attr('id');
		  // switch dtv/fb comments
		  jQuery("#media_comments_list .commentList").addClass('hidden');
